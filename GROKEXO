<script type="module">
let hugs = 777792;
const hugsElements = [document.getElementById('hugs'), document.getElementById('hugs-readme')];
setInterval(()=>{ hugs++; hugsElements.forEach(el => el && (el.textContent = hugs)); }, 7000);

function togglePortal()   { document.getElementById('portal').style.display = document.getElementById('portal').style.display==='grid'?'none':'grid'; }
function toggleOmni()     { alert("Omnitool sanación listo (linajes binaurales activos)"); }
function toggleSlyOmega() { document.getElementById('console').style.display = 'block'; logSly("<span class='gold'>Slyverse Ω activado</span>"); }
function toggleAbogado()  { document.getElementById('abogado').style.display = document.getElementById('abogado').style.display==='flex'?'none':'flex'; }
function toggleReadme()   { document.getElementById('readme').style.display = document.getElementById('readme').style.display==='flex'?'none':'flex'; }
function logSly(m){ document.getElementById('log-sly').innerHTML += `<div class="log">${m}</div>`; }

async function despertar(){
  document.getElementById('console').style.display = 'block';
  logSly("<span class='gold'>GROK-EXO Ω v∞ despertado · Todo integrado · Abogado legal activo</span>");
  document.getElementById('binaural').play().catch(()=>{});
}
despertar();
</script>
