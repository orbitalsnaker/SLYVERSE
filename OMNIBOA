function f13(){ 
  $('#app').innerHTML = `<div class="header"><button class="btn" onclick="menu()">←</button></div><h2>Bitcoin Wallet Offline</h2><div id="wallet"></div><button class="btn" onclick="newWallet()">GENERAR NUEVA WALLET</button>`;
  const s=document.createElement('script');s.src='https://unpkg.com/bitcoinjs-lib@6.1.0/dist/bitcoinjs-lib.min.js';document.head.appendChild(s);
  window.newWallet = () => {
    const network = bitcoin.networks.bitcoin;
    const keyPair = bitcoin.ECPair.makeRandom();
    const { address } = bitcoin.payments.p2pkh({ pubkey: keyPair.publicKey, network });
    const wif = keyPair.toWIF();
    $('#wallet').innerHTML = `<p>Address: <b>${address}</b><br>Private Key: <b style="color:#f00">${wif}</b><br>¡Guardala en papel!</p>`;
  };
}
