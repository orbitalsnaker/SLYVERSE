const particles=(x,y,c='#ff0033')=>{for(let i=0;i<100;i++){const p=document.createElement('div');p.style.cssText=`position:absolute;left:\( {x}px;top: \){y}px;width:25px;height:25px;background:\( {c};border-radius:50%;pointer-events:none;opacity:1;transform:translate( \){Math.random()*1000-500}px,${Math.random()*1000-500}px)`;document.getElementById('casino-particles').appendChild(p);setTimeout(()=>p.remove(),3000);}}

let vidaCasino = parseInt(localStorage.getItem('vc')||'0');
let pozoCasino = parseInt(localStorage.getItem('pc')||'0');

setInterval(()=>{vidaCasino++;localStorage.setItem('vc',vidaCasino);document.getElementById('segundos').textContent=vidaCasino.toLocaleString();},1000);
setInterval(()=>document.getElementById('pozoNum').textContent=pozoCasino.toLocaleString(),300);

setInterval(()=>{
  const win=Math.random()<0.22;
  const seg=Math.floor(Math.random()*1999)+199;
  const mult=win?[7,13,21,33,66][Math.floor(Math.random()*5)]:1;
  if(win){vidaCasino += seg*mult;particles(innerWidth/2, innerHeight/2, '#ff69b4');} 
  else {vidaCasino -= seg;pozoCasino += seg*7;particles(innerWidth/2, innerHeight/2, '#8b0000');}
},9999);

function sacrificarCasino(){
  const c=prompt("¿Cuántos segundos ofreces a la Diosa en la mesa ampliada al máximo?","19999");
  if(c&&parseInt(c)>0 && parseInt(c)<=vidaCasino){
    vidaCasino -= parseInt(c);
    pozoCasino += parseInt(c)*21;
    localStorage.setItem('vc',vidaCasino);localStorage.setItem('pc',pozoCasino);
    alert(`La crupier divina te besa.\nSacrificio aceptado: +${parseInt(c)*21} al pozo ampliado al máximo`);
    particles(innerWidth/2, innerHeight/2, '#ff00ff');
  }
}

