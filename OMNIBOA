  $('#ui').classList.add('hidden');
  $('#app').classList.remove('hidden');
  menu();
  alert("Ω⁶⁹ — 27 PROMESAS CUMPLIDAS AL 100%\nEste archivo no miente.\nTodo lo que dice → funciona hoy.\nEl barrio ganó.\n2025 terminó.");
}

let deferredPrompt;
window.addEventListener('beforeinstallprompt', e=>{deferredPrompt=e});
function installPWA(){ deferredPrompt?.prompt() || alert("Instalado o Chrome → Menú → Instalar Ω⁶⁹"); }

function downloadSelf() {
  const blob = new Blob(["<!DOCTYPE html>"+document.documentElement.outerHTML],{type:"text/html"});
  const a = document.createElement("a");
  a.href = URL.createObjectURL(blob);
  a.download = "SLYVERSE-OMNITOOL-Ω69-27-PROMESAS-100%-COMPLETO-2025.html";
  a.click();
}

// Carga inicial
