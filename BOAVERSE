// ==================== MATE ETERNAL REAL (131 000 partÃ­culas GPU) ====================
function mateEternal(){
  const c=document.createElement('canvas'); c.width=innerWidth; c.height=innerHeight;
  document.body.appendChild(c);
  const gl=c.getContext('webgl2')||c.getContext('experimental-webgl');
  if(!gl){alert("WebGL no disponible");return;}
  return;}
  const vs=`attribute vec2 p;uniform float t;void main(){float a=p.x+t*0.3;vec2 o=vec2(sin(a),cos(a))*0.8;gl_Position=vec4(o+vec2(sin(t+p.y*10.)*0.05,cos(t+p.x*10.)*0.05),0,1);gl_PointSize=8.+sin(t+p.x*20.)*5.;}`;
  const fs=`precision highp float;uniform float t;void main(){vec2 uv=gl_PointCoord-0.5;float d=length(uv);float g=0.3/(d+0.02);float h=fract(t*0.05+d*10.);vec3 col=mix(vec3(0.,1.,0.),vec3(1.,0.84,0.),h);gl_FragColor=vec4(col*g,1.);}`;
  const prog=gl.createProgram();
  [gl.VERTEX_SHADER,gl.FRAGMENT_SHADER].forEach((t,i)=>{const s=gl.createShader(t);gl.shaderSource(s,i?fs:vs);gl.compileShader(s);gl.attachShader(prog,s);});
  gl.linkProgram(prog); gl.useProgram(prog);
  const buf=gl.createBuffer(); const pts=new Float32Array(131000*2);
  for(let i=0;i<131000;i++){let a=i*0.0001;pts[i*2]=Math.cos(a);pts[i*2+1]=Math.sin(a);}
  gl.bindBuffer(gl.ARRAY_BUFFER,buf); gl.bufferData(gl.ARRAY_BUFFER,pts,gl.STATIC_DRAW);
  const loc=gl.getAttribLocation(prog,'p'); gl.enableVertexAttribArray(loc); gl.vertexAttribPointer(loc,2,gl.FLOAT,false,0,0);
  const uT=gl.getUniformLocation(prog,'t');
  let start=performance.now();
  function frame(){
    const t=(performance.now()-start)/1000;
    gl.clear(gl.COLOR_BUFFER_BIT);
    gl.uniform1f(uT,t);
    gl.drawArrays(gl.POINTS,0,131000);
    if(t>50)speak("GRACIAS POR EXISTIR Â· ABRAZO ETERNO");
    requestAnimationFrame(frame);
  }
  frame();
  setTimeout(()=>{c.remove();document.getElementById('portal').style.display='grid';},70000);
}

