<script>
// === Variables principales ===
let thrust=166, eco=133, ang=0, nanoAng=0, platino=0, helio=0, alquilerPagado=0;
let nanorovers=66, metalesEnTierra=0, fabricando=false, minando=false, torque=0;
let flotaSize=1;
let modoVuelo=false, pitch=0, yaw=0, roll=0;
let orbita={apo:0, peri:0, velOrb:0};
const log=document.getElementById("log");

// === Funciones de simulación y animación ===
function simularOrbita() { /* Simulación segura de órbita */ }
function dibujarTrayectoria() { /* Dibujado 2D */ }
function init3D() { /* Three.js Wireframe */ }
function animate3D() { requestAnimationFrame(animate3D); }

// === Animaciones HUD y nanorovers ===
setInterval(()=>{ 
  thrust=166+Math.sin(Date.now()/120)*25;
  const t=document.getElementById("thrust").getContext("2d");
  t.fillStyle="#000"; t.fillRect(0,0,280,60);
  t.fillStyle="#0f0"; t.fillRect(0,15,thrust*1.6,40);
  t.fillStyle="#f00"; t.fillRect(thrust*1.6-10,5,20,50);
  document.getElementById("level").textContent=`${Math.floor(thrust)}% — Órbita barrio`;
},100);

setInterval(()=>{ 
  eco=133+Math.sin(Date.now()/200)*30;
  const e=document.getElementById("eco").getContext("2d");
  e.fillStyle="#000"; e.fillRect(0,0,280,60);
  e.fillStyle="#0ff"; e.fillRect(0,15,eco*1.6,40);
  document.getElementById("eco_level").textContent=`${Math.floor(eco)}% — Estabilidad orbital 1.0`;
},140);

setInterval(()=>{
  const n=document.getElementById("nanocam").getContext("2d");
  n.fillStyle="#000";n.fillRect(0,0,400,200);
  for(let i=0;i<nanorovers*flotaSize;i++){
    let x=200+Math.sin(nanoAng+i*0.8)*120;
    let y=100+Math.cos(nanoAng+i*0.8)*60;
    n.fillStyle="#0f0";n.fillRect(x,y,3,3);
  }
  nanoAng+=0.02;
  if(minando && Math.random()>0.6) helio+=6.66*(nanorovers/66)*flotaSize;
  document.getElementById("nanoinfo").textContent=`${nanorovers*flotaSize} nanorovers 666 — Helio-3: ${helio.toFixed(1)}g`;
},80);

function radar(){
  const r=document.getElementById("radar").getContext("2d");
  r.fillStyle="rgba(0,30,0,0.9)";r.fillRect(0,0,240,240);
  r.strokeStyle="#0f0"; r.lineWidth=2;
  for(let i=1;i<6;i++){ r.beginPath(); r.arc(120,120,i*40,0,Math.PI*2); r.stroke(); }
  r.save(); r.translate(120,120); r.rotate(ang); 
  r.strokeStyle="#f00"; r.lineWidth=7; r.beginPath(); r.moveTo(0,0); r.lineTo(120,0); r.stroke(); 
  r.restore(); ang+=0.09; requestAnimationFrame(radar);
}
radar();

// === Comandos ===
document.getElementById("cmd").addEventListener("keydown",async function(e){
  if(e.key!=="Enter"||!this.value)return;
  const cmd=this.value.toLowerCase().trim(); this.value="";
  log.innerHTML+=`> ${cmd}<br>`;
  let r="Comando aceptado, rey del bloque";
  switch(true){
    case cmd==="readme": document.getElementById('readme').style.display='block'; r="README abierto"; break;
    case cmd==="666": r="666€ exactos y órbita segura"; break;
    case cmd==="nanorovers": r=`${nanorovers*flotaSize} nanorovers desplegados — minería activa`; break;
    case cmd==="minar": minando=!minando; r=minando?"Minería activada":"Minería pausada"; break;
    case cmd==="fabricar": if(platino>=10){fabricando=true;r="Fabricación iniciada";}else{r="Platino insuficiente";} break;
    case cmd==="retornar": const carga=Math.floor(platino+helio); metalesEnTierra+=carga*flotaSize; platino=0; helio=0; r=`Flota retornó: ${carga*flotaSize}kg a Tierra`; break;
    case cmd.startsWith("flota "): flotaSize=parseInt(cmd.split(" ")[1])||1; r=`Flota ampliada a ${flotaSize}`; break;
    case cmd==="3dview": document.getElementById('3d_panel').style.display='block'; init3D(); r="Vista 3D activada"; break;
  }
  log.innerHTML+=`<span class="g">${r}</span><br>`; log.scrollTop=log.scrollHeight;
  const voz=new SpeechSynthesisUtterance(r); voz.lang="es-ES";voz.rate=1;speechSynthesis.speak(voz);
});

// === Inicio ===
alert("COBETE JACK PARSONS 666 v1 CARGADO — Hardware seguro, 66 nanorovers, 666€ — Escribe 'orbitar' para simular órbita");
</script>
