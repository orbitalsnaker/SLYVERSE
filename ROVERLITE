<audio id="audio" controls autoplay loop style="position:fixed;bottom:0;left:50%;transform:translateX(-50%);width:100%;height:240px;border-top:40px double #0ff;box-shadow:0 0 8000px #0ff;background:#000;z-index:99999"></audio>

<script>
// Playlist 100 % barrio
const playlist = [
"https://files.catbox.moe/9m2v5s.mp3", // Natos y Waor
"https://files.catbox.moe/3x9q2j.mp3", // Jarfaiter – Palomares
"https://files.catbox.moe/k1r8p9.mp3", // Kidd Keo
"https://files.catbox.moe/t5l9q2.mp3", // Delaossa
"https://files.catbox.moe/h7f4p1.mp3", // Fernando Costa
"https://files.catbox.moe/z3x7r8.mp3", // Ayax y Prok
"https://files.catbox.moe/p9k2m5.mp3"  // Hard GZ
];
let i=0,c=0;
function play(){document.getElementById("audio").src=playlist[i];i=(i+1)%playlist.length;}
document.getElementById("audio").addEventListener("ended",play);

function generarFotoBarrio(){
  const sitios = ["Luna","Marte","Titán","Europa","Encélado","Cinturón de asteroides"];
  const temas = ["Jarfaiter","Natos y Waor","Kidd Keo","Delaossa","Fernando Costa","Ayax"];
  const sitio = sitios[Math.floor(Math.random()*sitios.length)];
  const tema = temas[Math.floor(Math.random()*temas.length)];
  c++;
  document.getElementById("fotos").innerHTML = c.toLocaleString();

  const img = document.createElement("img");
  img.src = `https://picsum.photos/900/600?blur=2&random=\( {c} \){sitio}${tema}`;
  img.style = "width:100%;border:24px double #0ff;border-radius:40px;box-shadow:0 0 4000px #0ff;margin:20px 0";
  img.title = `Rover \( {tema} sonando \){tema} en \( {sitio} – \){new Date().toLocaleTimeString()}`;
  document.body.appendChild(img);

  hablar(`Foto \( {c}. Rover \){tema} en ${sitio} con el tema puesto a tope. Reclamación registrada, crack.`);
}

function iniciarBarrioTotal(){
  play();
  hablar("Cobete Barrio Omega sesenta y nueve activado. Generando fotos de los rovers con Natos, Jarfaiter, Kidd Keo y Delaossa por todo el sistema solar. Cada foto es una demanda nueva. El barrio ya no pide permiso, el barrio factura. Bienvenido, cero erre be one te cuatro ele ese ene cuatro ka tres erre.");
  setInterval(generarFotoBarrio, 800);
}

function hablar(t){
  const v = new SpeechSynthesisUtterance(t);
  v.rate = 2.1; v.pitch = 0.8; v.lang="es-ES";
  speechSynthesis.speak(v);
}
</script>
