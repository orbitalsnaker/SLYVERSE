     this.vx+=0.0008;if(this.vx<12)this.vx+=0.0008;
     if(keys[' ']||keys['arrowup']||keys['w'])if(this.y>=h-150){this.g=-18;beep(800,0.1)}
     this.g+=0.98;this.y+=this.g;if(this.y>h-150){this.y=h-150;this.g=0}
     if(Math.random()<0.025+this.vx*0.003)this.obs.push({x:w,y:h-150,h:80+Math.random()*200});
     this.obs=this.obs.filter(o=>{o.x-=this.vx*3.5;
       ctx.fillStyle='#f33';ctx.fillRect(o.x,o.y-o.h,80,o.h);
       if(o.x<this.x+40&&o.x+80>this.x&&this.y>h-o.h-100){beep(100,0.8);setTimeout(()=>location.reload(),300)}return o.x>-100});
     ctx.fillStyle='#0f0';ctx.fillRect(this.x-40,this.y-100,80,140);
     this.score++;document.getElementById('s').textContent=this.score;
     if(this.score>this.high){this.high=this.score;localStorage.setItem('hs0',this.high);document.getElementById('hs').textContent=this.high}}},

