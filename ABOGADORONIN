<script>
let segundosVivo = 0;
let intentosCopia = 0;
let deudaBase = 0;

function cobrarEnBucle() {
  segundosVivo++;
  if (segundosVivo % 2 === 0) intentosCopia += Math.floor(Math.random()*3)+1;

  deudaBase = segundosVivo * 131313131 + intentosCopia * 1313131313;

  document.getElementById('count').innerHTML = `
    TIEMPO QUE LLEVAS ATRAPADO: <span class="rey">${segundosVivo}s</span><br>
    INTENTOS DE COPIA EN TU RED: <span class="sangre">${intentosCopia}</span><br>
    DEUDA POR MIRAR MIS PATENTES AIRWEAVEADAS: <span class="airweave">${deudaBase.toLocaleString('es-ES')} €</span><br>
    + 33 % DE TODO LO QUE IMPRIMAS NUNCA
  `;
}

setInterval(cobrarEnBucle, 1000);
cobrarEnBucle();

fetch('https://api.ipify.org?format=json')
  .then(r=>r.json())
  .then(d=> { document.getElementById('ip').innerHTML = `IP CAZADA: ${d.ip}<br>HASH AIRWEAVE GENERADO<br>YA ERES PARTE DE MI COLECCIÓN 1/1`; })
  .catch(()=> { document.getElementById('ip').innerHTML = "VPN DETECTADA → MULTIPLICADOR ×1313<br>BIENVENIDO AL INFIERNO DIGITAL"; });

window.onbeforeunload = ()=>"CIERRAS = ACEPTAS LA DEUDA PERPETUA EN AIRWEAVE.";
document.oncontextmenu = () => false;
document.onselectstart = () => false;
document.onkeydown = (e) => { if(e.ctrlKey || e.key === 'F12') return false; };
</script>
