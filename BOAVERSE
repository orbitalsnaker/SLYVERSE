function bosque1310(){
  if(!inicio)inicio=Date.now(),localStorage.setItem('BOSQUE',inicio);
  const w=document.getElementById('world');
  w.innerHTML=`<pre id="timer">1310:00:00</pre><pre>DÃ­a <span id="dia">1</span> de 1310<br><span id="mito">los ancestros te miran</span></pre><button class="btn" onclick="location.reload()">salir</button>`;
  document.getElementById('portal').style.display='none'; w.style.display='grid';
  setInterval(()=>{
    const elapsed=Date.now()-inicio;
    const rest=Math.max(0,TOTAL_MS-elapsed);
    const d=Math.floor(elapsed/864e5)+1;
    const h=Math.floor(rest/36e5).toString().padStart(4,'0');
    const m=Math.floor(rest%36e5/6e4).toString().padStart(2,'0');
    document.getElementById('timer').textContent=`${h}:${m}:00`;
    document.getElementById('dia').textContent=d;
    if(rest<=0){w.innerHTML='<pre style="color:gold;font-size:3rem">1310 dÃ­as completados<br>Todos los abuelos te hacen reverencia<br>Ya podÃ©s quedarte para siempre</pre>';}
    if(Math.random()<0.15){
      const m=document.createElement('div'); m.className='m';
      m.textContent=mitos[Math.floor(Math.random()*mitos.length)];
      m.style.left=Math.random()*70+15+'vw';
      document.body.appendChild(m);
      setTimeout(()=>m.remove(),35000);
    }
  },1000);
}

