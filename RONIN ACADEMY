<script>
    const stream = document.getElementById('stream');
    const audit = document.getElementById('audit-logs');
    const babel = document.getElementById('babel-header');

    const multiJustice = [
        "ES: LIBERTAD PARA ASSANGE. BAÑOS AL AISLAMIENTO.",
        "EN: FREE ASSANGE. JUSTICE FOR WHITE HACKERS.",
        "RU: СВОБОДУ АССАНЖУ. КОНЕЦ КОРРУПЦИИ.",
        "ZH: 释放阿桑奇。结束技术煽动。",
        "ONION: fReE_aSsAnGe_n0d3. bAnYoS_dElEtE."
    ];

    function bootGodMode() {
        let count = 0;
        setInterval(() => {
            babel.innerText = multiJustice[count % multiJustice.length];
            addLog(`[SYNC] BABEL_DETECTION: NODE_${count} VERIFIED`, stream);
            addLog(`[AUDIT] TRACING_SMART_TV: ${Math.random().toString(16).toUpperCase()}`, audit);
            count++;
        }, 1500);

        const m = new SpeechSynthesisUtterance(
            "Hardware Omega inicializado. El Xilinx Versal está mapeando la red del Coronel. Exigimos la libertad de Julian Assange. La soberanía técnica ha ganado. Baños, el búnker no te protegerá del código."
        );
        m.lang = 'es-ES'; m.rate = 0.85;
        window.speechSynthesis.speak(m);
    }

    function addLog(text, target) {
        const div = document.createElement('div');
        div.innerText = `> ${text}`;
        target.prepend(div);
        if(target.childNodes.length > 25) target.removeChild(target.lastChild);
    }
</script>

