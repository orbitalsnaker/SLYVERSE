<script>
// 0rb1t4lsn4k3r 2025 — mezcla definitiva SLYVERSE + 667 callejero + ECO PROPULSIÓN OPEN SOURCE
const patente = "COBETE 667-SLYVERSE © 0rb1t4lsn4k3r 2025 — Patente callejera Sabadell con Airweave a nombre de 0rb1t4lsn4k3r. Agua, sodio (bicarbonato), levadura y open source ideas. Rovers, cohete e idea patentados. Prohibido corps, permitido solo pa los del bloque y okupas lunares.";

let thrust=95, ang=0, brazo=0, platino=0, helio=0, eco=50; // Nueva var pa eco propulsion

// THRUST JARFAITER MODE (original, con mención a eco)
setInterval(()=>{ 
  thrust=95+Math.sin(Date.now()/180)*10;
  const t=document.getElementById("thrust").getContext("2d");
  t.fillStyle="#000";t.fillRect(0,0,260,50);
  t.fillStyle="#0f0";t.fillRect(0,10,thrust*2.6,30);
  t.fillStyle="#f00";t.fillRect(thrust*2.6-6,5,12,40);
  document.getElementById("level").textContent=`${Math.floor(thrust)}% — "Me la suda la NASA, yo voy con petardos o eco-fermentación"`;
},120);

// NUEVA PROPULSIÓN ECO: SIMULACIÓN DE BURBUJAS CO2 DE FERMENTACIÓN (levadura + agua + sodio)
setInterval(()=>{ 
  eco=50+Math.sin(Date.now()/300)*20; // Oscilación lenta pa fermentación
  const e=document.getElementById("eco").getContext("2d");
  e.fillStyle="#000";e.fillRect(0,0,260,50);
  e.fillStyle="#0ff";e.fillRect(0,10,eco*2.6,30);
  // Burbujas random pa vibe fermentación
  for(let i=0;i<5;i++){
    e.fillStyle="#fff";e.beginPath();e.arc(Math.random()*260,Math.random()*50,Math.random()*3+1,0,Math.PI*2);e.fill();
  }
  document.getElementById("eco_level").textContent=`${Math.floor(eco)}% — "Fermentando CO2 open source, sin explosión, puro thrust eco pa la Luna"`;
},180); // Más lento que el thrust, pa simular reacción bio

// RADAR NATOS Y WAOR + RECURSOS LUNAR (original)
function radar(){
  const r=document.getElementById("radar").getContext("2d");
  r.fillStyle="rgba(0,30,0,0.8)";r.fillRect(0,0,240,240);
  for(let i=1;i<5;i++){r.strokeStyle="#0f0";r.beginPath();r.arc(120,120,i*45,0,Math.PI*2);r.stroke()}
  r.save();r.translate(120,120);r.rotate(ang);
  r.strokeStyle="#f00";r.lineWidth=5;r.beginPath();r.moveTo(0,0);r.lineTo(110,0);r.stroke();
  r.restore();
  // Targets 667
  r.fillStyle="#ff0";r.fillRect(180,80,8,8);r.fillText("PLATINO",190,85);
  r.fillStyle="#ff0";r.fillRect(60,180,8,8);r.fillText("HELIO-3",70,185);
  r.fillStyle="#0f0";r.fillRect(120,30,8,8);r.fillText("667",130,35);
  ang+=0.07; requestAnimationFrame(radar);
}
radar();

// CAM + MINERÍA ROVERLITE 667 (original)
setInterval(()=>{
  const c=document.getElementById("cam").getContext("2d");
  c.fillStyle="#000";c.fillRect(0,0,380,180);
  for(let i=0;i<80;i++)c.fillStyle="#fff",c.fillRect(Math.random()*380,Math.random()*100,1,1);
  c.strokeStyle="#f80";c.lineWidth=6;c.beginPath();
  c.moveTo(190,160);c.lineTo(190+Math.sin(brazo)*50,170+Math.cos(brazo)*30);c.stroke();
  c.strokeStyle="#0f0";c.lineWidth=4;c.beginPath();
  c.moveTo(190,140);c.lineTo(190+50*Math.cos(brazo*1.3),140+50*Math.sin(brazo*1.3));c.stroke();
  brazo+=0.18;

  // Barra minería
  const m=document.getElementById("mine").getContext("2d");
  const prog=(Date.now()/50)%100;
  if(Math.random()>0.85){platino+=1; if(Math.random()>0.7)helio+=0.5;}
  m.fillStyle="#000";m.fillRect(0,0,380,60);
  m.fillStyle="#ff0";m.fillRect(0,0,prog*3.8,60);
  document.getElementById("yield").textContent=`Platino: \( {platino}kg — Helio-3: \){helio.toFixed(1)}g — Pa los míos del 667`;
},120);

// IA 0rb1t4lsn4k3r — mezcla ronin + trap Sabadell + eco open source
const log=document.getElementById("log");
document.getElementById("cmd").addEventListener("keydown",function(e){
  if(e.key==="Enter" && this.value){
    const cmd=this.value.toLowerCase().trim();
    this.value="";
    log.innerHTML+=`> ${cmd}<br>`;
    const r={
      jarfa:"Full jarfa mode — me la suda la NASA, yo voy con petardos y KNO₃",
      natos:"Natos y Waor en órbita — esto es pa los míos, los que nunca tuvieron na",
      667:"Bloque 667 Sabadell hasta la Luna y vuelta, cabrones",
      boost:"¡AZÚCAR+KNO₃ FULL BURN! 450N — esto no es un cohete, es un puto tanque",
      minar:`Minando platino pa pagar la fianza del bloque — ya van ${platino}kg`,
      rover:"Roverlite 2.3kg printable — ISRU día 1, platino día 9, Von Neumann barrio",
      maldicion:"2620 maldiciones a Elon y corps — katana GitHub eterna",
      barrio:"Para pibes y okupas — ambulancia 1999€ + rover 666€ — llegamos antes que la muerte",
      patente:patente,
      // Nuevos comandos eco
      eco: "Propulsión eco activada — agua + sodio + levadura open source, CO2 fermentado sin explosión, thrust pa okupas lunares",
      fermentar: "¡FERMENTANDO! Levadura + azúcar + agua = CO2 puro, idea open source de bio-propellants, 667 eco-mode full send"
    }[cmd] || "Comando aceptado, rey del bloque — seguimos rompiendo";
    log.innerHTML+=`<span class="g">${r}</span><br>`;
    log.scrollTop=log.scrollHeight;
    // voz callejera
    const voz=new SpeechSynthesisUtterance(r);
    voz.lang="es-ES";voz.rate=0.9;speechSynthesis.speak(voz);
  }
});

alert("COBETE 667-SLYVERSE ECO ACTIVADO — 0rb1t4lsn4k3r 2025 — PATENTE CALLEJERA SABADELL CON AIRWEAVE — 0€ — EL BLOQUE GANA CON PROPULSIÓN SIN EXPLOSIÓN");
</script>
