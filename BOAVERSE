// === THUAMER v1 ORIGINAL (18.1 KB alma pura) ===
const rosetta=()=>{mode='rosetta';document.getElementById('rosetta').classList.add('show');
  const p=[];for(let i=0;i<8888;i++)p.push({a:Math.random()*Math.PI*2,d:80+Math.random()*444});
  gameLoop=()=>{t+=0.016;ctx.fillStyle='rgba(0,0,0,0.05)';ctx.fillRect(0,0,canvas.width,canvas.height);
    p.forEach(e=>{const angle=e.a+t*0.25;const dist=e.d+Math.sin(t*0.6+e.a)*90;const hue=120+Math.sin(t+e.a)*30;
      ctx.fillStyle=`hsla(${hue},100%,60%,0.7)`;ctx.fillRect(canvas.width/2+Math.cos(angle)*dist-3,canvas.height/2+Math.sin(angle)*dist-3,6,6);});
  };
  const scripts=document.querySelectorAll('#rosetta .script, #rosetta .dedicatoria, #rosetta div');scripts.forEach((s,i)=>{setTimeout(()=>{s.style.opacity=1;s.style.transform='translateY(0)'},i*1200)});
};

