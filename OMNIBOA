function thuamer() { $('#app').innerHTML = `<button class="btn" onclick="menu()">←</button><iframe src="https://thuamer.slyverse.isostatic.net" style="height:90vh"></iframe>`; }
function metaverse() { $('#app').innerHTML = `<button class="btn" onclick="menu()">←</button><iframe src="data:text/html,${encodeURIComponent('<!DOCTYPE html><html><head><script src="https://aframe.io/releases/1.5.0/aframe.min.js"></script></head><body><a-scene><a-box color="#0f0" position="-1 0.5 -3"></a-box><a-sphere color="#0ff" position="0 1.5 -5"></a-sphere><a-plane color="#003300" rotation="-90 0 0" width="20" height="20"></a-plane><a-sky color="#000"></a-sky></a-scene></body></html>')}"></iframe>`; }

