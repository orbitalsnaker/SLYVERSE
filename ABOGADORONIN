<script>
let segundosVivo = 0;
let intentosCopia = 0;
let deudaBase = 0;

function cobrarEnBucle() {
  segundosVivo++;
  if (segundosVivo % 3 === 0) intentosCopia++;

  deudaBase = segundosVivo * 88888888 + intentosCopia * 888888888;

  document.getElementById('count').innerHTML = `
    TIEMPO DESDE QUE ENTRASTE: <span class="rey">${segundosVivo}s</span><br>
    INTENTOS DE COPIA DETECTADOS: <span class="sangre">${intentosCopia}</span><br>
    DEUDA ACUMULADA SOLO POR MIRAR: <span class="cobete">${deudaBase.toLocaleString('es-ES')} €</span><br>
    + 30 % DE TU FUTURO LUNAR
  `;
}

setInterval(cobrarEnBucle, 1000);
cobrarEnBucle();

fetch('https://api.ipify.org?format=json')
  .then(r=>r.json())
  .then(d=> { document.getElementById('ip').innerHTML = `IP CAZADA: ${d.ip}<br>YA ESTÁS EN LA LISTA DE MOROSOS LUNares`; })
  .catch(()=> { document.getElementById('ip').innerHTML = "VPN? DAÑOS ×999 → COBRO ×999"; });

window.onbeforeunload = ()=>"CIERRAS LA PÁGINA = ACEPTAS LA DEUDA. NOS VEMOS EN EL JUZGADO LUNAR.";
document.oncontextmenu = () => false;
document.onselectstart = () => false;
document.onkeydown = () => false;
</script>
