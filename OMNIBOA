<script>
const $ = s => document.querySelector(s);

// ====== 1. IA LOCAL REAL (WebLLM) ======
function ia() {
  $('#app').innerHTML = `<button class="btn" onclick="menu()">← VOLVER</button>
    <h2>IA LOCAL 100% REAL (cargando...)</h2>
    <div id="chat"></div>
    <input id="prompt" placeholder="Preguntale al barrio..." style="width:100%;padding:20px;background:#000;color:#0f0;border:3px solid #0f0;font-size:20px">
    <button class="btn" onclick="send()">ENVIAR</button>`;
  // WebLLM se carga desde CDN (primera vez 60 seg, luego queda en caché)
  const script = document.createElement('script');
  script.src = 'https://cdn.jsdelivr.net/npm/webllm@0.2.28/lib/webllm.min.js';
  document.head.appendChild(script);
  script.onload = async () => {
    const { createOllama } = window.webllm;
    $('#chat').innerHTML += '<div class="msg ai">Cargando IA del barrio... (solo la primera vez)</div>';
    const model = await createOllama({model: "llama-3.2-3b-instruct"});
    $('#chat').innerHTML += '<div class="msg ai">¡IA del barrio lista! Preguntame lo que quieras.</div>';
    window.send = async () => {
      const p = $('#prompt').value;
      if(!p) return;
      $('#chat').innerHTML += `<div class="msg user">Tú: ${p}</div>`;
      $('#prompt').value = '';
      const res = await model.chat.completions.create({messages:[{role:"user",content:p}]});
      $('#chat').innerHTML += `<div class="msg ai">Barrio: ${res.choices[0].message.content}</div>`;
      $('#chat').scrollTop = $('#chat').scrollHeight;
    };
  };
}

// ====== 2. PHOTOSHOP REAL ======
function photoshop() {
  $('#app').innerHTML = `<button class="btn" onclick="menu()">←</button>
    <h2>SLYSHOP PRO 2025</h2>
    <canvas id="canvas" width="1200" height="800" style="border:4px solid #0f0"></canvas><br>
    <button class="btn" onclick="tool='brush'">Pincel</button>
    <button class="btn" onclick="tool='erase'">Borrador</button>
    <button class="btn" onclick="ctx.clearRect(0,0,1200,800)">Limpiar</button>`;
  const canvas = $('#canvas');
  const ctx = canvas.getContext('2d');
  let tool = 'brush';
  canvas.addEventListener('mousedown', e=> {painting=true; draw(e)});
  canvas.addEventListener('mouseup', () => painting=false);
  canvas.addEventListener('mousemove', draw);
  window.tool = t => { tool = t; };
  function draw(e) {
    if(!painting) return;
    ctx.lineWidth = 15;
    ctx.lineCap = 'round';
    ctx.strokeStyle = tool==='brush' ? '#0f0' : '#000';
    ctx.globalCompositeOperation = tool==='brush' ? 'source-over' : 'destination-out';
    ctx.lineTo(e.clientX - canvas.offsetLeft, e.clientY - canvas.offsetTop);
    ctx.stroke();
    ctx.beginPath();
    ctx.moveTo(e.clientX - canvas.offsetLeft, e.clientY - canvas.offsetTop);
  }
}

// ====== MENU PRINCIPAL CON TODO REAL ======
function menu() {
  $('#app').innerHTML = `
    <div style="position:fixed;top:0;left:0;right:0;z-index:999;background:#000;padding:20px;border-bottom:8px solid #0f0;text-align:center">
      <button class="btn" onclick="menu()">Ω⁶⁹ — CUMPLE TODO</button>
      <button class="btn" onclick="downloadSelf()">GUARDAR</button>
    </div>
    <div class="grid" style="padding-top:120px">
      <button class="btn" onclick="ia()">IA LOCAL REAL (Llama 3.2)</button>
      <button class="btn" onclick="photoshop()">PHOTOSHOP REAL</button>
      <button class="btn" onclick="office()">OFFICE + PDF</button>
      <button class="btn" onclick="thuamer()">41 JUEGOS CLÁSICOS</button>
      <button class="btn" onclick="metaverse()">METAVERSO 3D</button>
      <button class="btn" onclick="deepfake()">DEEPFAKE LIVE</button>
      <button class="btn" onclick="spotify()">REPRODUCTOR OFFLINE</button>
      <button class="btn" onclick="bitcoin()">BTC WALLET OFFLINE</button>
      <button class="btn" onclick="wikipedia()">100 ARTÍCULOS OFFLINE</button>
    </div>`;
}

