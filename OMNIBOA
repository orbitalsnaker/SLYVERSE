function downloadSelf(){
  const blob = new Blob(["<!DOCTYPE html>"+document.documentElement.outerHTML],{type:"text/html"});
  const a = document.createElement("a");
  a.href = URL.createObjectURL(blob);
  a.download = "SLYVERSE-Ω69-FINAL-ABSOLUTO-27-DE-27.html";
  a.click();
}

if('serviceWorker' in navigator){
  navigator.serviceWorker.register(URL.createObjectURL(new Blob(["self.addEventListener('install',e=>e.waitUntil(caches.open('sly69').then(c=>c.addAll(['.']))))"],{type:'application/javascript'})));
