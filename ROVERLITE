<script>
const log = document.getElementById("log");
document.getElementById("cmd").addEventListener("keypress", e => {
  if (e.key === "Enter") {
    let cmd = e.target.value.trim().toLowerCase();
    if (cmd === "readme") document.getElementById('readme').style.display = 'block';
    if (cmd === "patents") log.innerHTML += "<br>28 patents de cobeteria carregades · CAT-001 a CAT-222 · Propietat exclusiva 0rb1t4lsn4k3r<br>";
    if (cmd === "buy15") log.innerHTML += "<br>15× cobete v19-GRIS v2.1 → 9 990,00 € → <a style='color:#ff00ff' href='https://0rb1t4lsn4k3r.shop/v2.1' target='_blank'>BASE LUNAR EXPRESS</a><br>";
    if (cmd === "kaboom") log.innerHTML += "<br>SNAKEPREDICT v9: Probabilitat explosió = 0,000 % (ni amb C4 i mala llet)<br>";
    if (cmd === "cobeteria") log.innerHTML += "<br>COBETERIA ACTIVADA: la coheteria està morta · Visca els cobetes de barri!<br>";
    log.scrollTop = log.scrollHeight;
    e.target.value = "";
  }
});

// Motor 3D ultra cabrón
let scene, camera, renderer, engineGroup;
function init3D() {
  scene = new THREE.Scene(); scene.background = new THREE.Color(0x110022);
  camera = new THREE.PerspectiveCamera(65, innerWidth/innerHeight, 0.1, 1000);
  camera.position.set(45, 35, 70);
  renderer = new THREE.WebGLRenderer({antialias:true, alpha:true});
  renderer.setSize(innerWidth, innerHeight);
  document.getElementById("threejs_container").appendChild(renderer.domElement);
  new THREE.OrbitControls(camera, renderer.domElement);
  scene.add(new THREE.AmbientLight(0xff00ff, 5));

  engineGroup = new THREE.Group();
  const geo1 = new THREE.CylinderGeometry(3.2, 3.8, 28, 48);
  const geo2 = new THREE.ConeGeometry(3.8, 34, 48);
  const mat1 = new THREE.MeshPhongMaterial({color:0xff00ff, emissive:0xff00aa, emissiveIntensity:6, metalness:0.9});
  const mat2 = new THREE.MeshPhongMaterial({color:0xaa00ff, emissive:0xcc00cc, emissiveIntensity:5});
  const chamber = new THREE.Mesh(geo1, mat1);
  const nozzle = new THREE.Mesh(geo2, mat2);
  nozzle.position.y = -30;
  engineGroup.add(chamber); engineGroup.add(nozzle);
  scene.add(engineGroup);

  let t = 0;
  function loop() {
    requestAnimationFrame(loop);
    t += 0.06;
    engineGroup.rotation.y = Math.sin(t)*0.5;
    engineGroup.position.y = Math.sin(t*7)*2.5;
    engineGroup.scale.y = 1 + Math.sin(t*20)*0.05;
    renderer.render(scene, camera);
  }
  loop();
}
init3D();
</script>
