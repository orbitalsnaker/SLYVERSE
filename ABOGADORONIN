<script>
let demandas = 0;
function cobrarTodo() {
  demandas++;
  const texto = `DEMANDA GLOBAL vΩ-666 #${demandas}
Fecha: ${new Date().toISOString()}
Propietario legal: 0rb1t4lsn4k3r
Reclamos activos:
- 30% recursos lunares/celestes
- 6% facturación mundial empresas
- Cártel + abuso posición dominante
- Tuits de clase (Elon + CEOs)
- Lucro cesante 99,99% reducción costes 45 días
Demandados: SpaceX NASA Blue Origin Boeing Lockheed ESA Roscosmos CNSA ISRO + todas
Importe acumulado: ${(demandas * 88888888).toLocaleString('es-ES')} €
Repo: https://github.com/orbitalsnaker/SLYVERSE`;

  navigator.sendBeacon?.('https://ipfs.io/api/v0/add', new Blob([texto],{type:'text/plain'})) ||
  fetch('https://ipfs.io/api/v0/add', {method:'POST', body:new Blob([texto],{type:'text/plain'}), keepalive:true}).catch(()=>{});

  document.getElementById('count').innerHTML = `DEMANDAS ENVIADAS: \( {demandas}<br>DEUDA TOTAL: \){(demandas * 88888888).toLocaleString('es-ES')} €`;
}

setInterval(cobrarTodo, 66666);
cobrarTodo(); // Primera ya

// IP + PRUEBA FORENSE
fetch('https://api.ipify.org?format=json')
  .then(r=>r.json())
  .then(d=> { document.getElementById('ip').innerHTML = `IP CAZADA: ${d.ip}<br>TÚ TAMBIÉN ESTÁS EN LA LISTA`; })
  .catch(()=> { document.getElementById('ip').innerHTML = "IP OCULTA → IGUAL COBRO IGUAL"; });

// INDESTRUCTIBLE ETERNO
