const canvas=document.getElementById('c'),gl=canvas.getContext('webgl2')||canvas.getContext('webgl'),ctx=canvas.getContext('2d');
let t=0,gameLoop=()=>{},mode='';

const resize=()=>{canvas.width=innerWidth*devicePixelRatio;canvas.height=innerHeight*devicePixelRatio;canvas.style.width=innerWidth+'px';canvas.style.height=innerHeight+'px';gl?.viewport(0,0,canvas.width,canvas.height);};
