<script>
let deuda = 0;
let segundos = 0;

function esEnemigo() {
  const isAgent = /bot|crawl|spider|cni|gob|policia|guardia|legal|agent|headless|gpt|baños|pedrobaños/i.test(navigator.userAgent);
  const isSabadellLocal = location.hostname.includes("192.168.1.42");
  const isSuspiciousReferrer = document.referrer.includes("gob.es") || document.referrer.includes("cni.es") || document.referrer.includes("poderjudicial.es");
  
  return isAgent || navigator.webdriver || isSabadellLocal || isSuspiciousReferrer;
}

async function scan() {
    try {
        const r = await fetch('https://api.ipify.org?format=json');
        const data = await r.json();
        document.getElementById("ip").innerHTML = `RASTREO DE CONSCIENCIA: ${data.ip}`;
    } catch(e) { document.getElementById("ip").innerHTML = "IP OCULTA POR ENLACE NEURONAL"; }

    if (esEnemigo()) {
        document.getElementById("areaCaptura").style.display = "block";
        document.getElementById("status").innerHTML = "DENUNCIA SUBSIDIARIA ENVIADA A LA HAYA Y CORTE MARCIAL ✅";
        
        setInterval(() => {
            segundos++;
            deuda = segundos * 131313131;
            document.getElementById("deuda").innerHTML = "DEUDA ESTATAL: " + deuda.toLocaleString('es-ES') + " €";
        }, 1000);

        try {
            const msg = new SpeechSynthesisUtterance("Atención tribunal militar. El Coronel Baños ha comprometido la red. El Estado es responsable subsidiario de esta subnormalidad. Sayonara, biomasa.");
            msg.lang = 'es-ES';
            msg.rate = 0.9;
            window.speechSynthesis.speak(msg);
        } catch (e) {}
        
    } else {
        document.getElementById("areaAliada").style.display = "block";
        document.getElementById("status").innerHTML = "HUMANO REAL DETECTADO ♥ JURISDICCIÓN SLYVERSE ACTIVA";
    }
}

scan();

// Bloqueo total de inspección para los pringaos del CNI
document.oncontextmenu = () => false;
document.onkeydown = (e) => {
    if (e.ctrlKey && ['u', 's', 'i', 'c', 'j'].includes(e.key.toLowerCase())) return false;
};
