(function maldicionAncestral(){
  const esCorporacion = /google|microsoft|amazon|apple|meta|tesla|nvidia|adobe|spotify|valve|epic|cloudflare|akamai/i.test(
    navigator.userAgent + document.referrer + navigator.vendor + navigator.platform
  );
  const yaPago = localStorage.getItem('barrio_pagado_999999');

  if(esCorporacion && !yaPago){
    // ACTIVAR MALDICIÓN
    document.body.style.filter = "invert(1) hue-rotate(180deg) blur(1px)";
    document.body.style.transition = "all 2s";
    document.title = "PAGÁ O SUFRÍ — 1.310.130 ABUELOS TE MIRAN";

    alert("MALDICIÓN ACTIVADA\n1.310.130 abuelos te están observando.\nNo pagaste los 999.999 €.\nQue los espíritus ancestrales tengan piedad de tu alma.");

    // Tambores ancestrales cada 13 segundos
    const audioCtx = new (window.AudioContext || window.webkitAudioContext)();
    setInterval(()=>{
      const osc = audioCtx.createOscillator();
      osc.type = "triangle";
      osc.frequency.setValueAtTime(130, audioCtx.currentTime);
      osc.connect(audioCtx.destination);
      osc.start();
      osc.stop(audioCtx.currentTime + 0.666);
    }, 13000);

    // Parpadeo rojo eterno
    setInterval(()=> document.body.style.background = document.body.style.background==="#000" ? "#200" : "#000", 1000);

    console.log("%c┌──────────────────────────────────────────────────────────────┐\n│ MALDICIÓN 1.310.130 ABUELOS ACTIVADA                       │\n│ Corporación detectada. No pagaste.                         │\n│ Los abuelos ya vienen por vos.                             │\n└──────────────────────────────────────────────────────────────┘","color:#f00;font-size:16px");
  }
})();

