function f1(){ /* IA Local Llama 3.2 3B */ 
  $('#app').innerHTML = `<div class="header"><button class="btn" onclick="menu()">←</button></div><h2>1. IA LOCAL LLAMA 3.2 3B</h2><div id="chat" style="height:60vh;overflow:auto;background:#000;padding:20px;border:4px double #0f0"></div>
    <input id="q" placeholder="Preguntale lo que quieras (100% local)" style="width:70%;padding:20px;background:#000;color:#0f0;border:6px solid #0f0;font-size:22px">
    <button class="btn" onclick="send()">ENVIAR</button><div id="status">Cargando modelo 3.2 GB... (solo la primera vez)</div>`;
  const s = document.createElement('script');
  s.src = 'https://cdn.jsdelivr.net/npm/@mlc-ai/web-llm@0.2.46/dist/webllm.min.js';
  document.head.appendChild(s);
  s.onload = async () => {
    const engine = await webllm.CreateMLCEngine("Llama-3.2-3B-Instruct-q4f16_1-MLC", {initProgressCallback: (p)=>$('#status').innerText = `Cargando... ${Math.round(p.progress*100)}%`});
    $('#status').innerText = "¡Modelo cargado! Preguntá lo que quieras";
    window.send = async () => {
      const msg = $('#q').value.trim(); if(!msg) return;
      $('#chat').innerHTML += `<div style="color:#0f0">» ${msg}</div>`; $('#q').value='';
      const reply = await engine.chat.completions.create({messages:[{role:"user",content:msg}]});
      $('#chat').innerHTML += `<div style="color:#0ff">Llama 3.2: ${reply.choices[0].message.content}</div>`;
      $('#chat').scrollTop = $('#chat').scrollHeight;
    };
  };
}

