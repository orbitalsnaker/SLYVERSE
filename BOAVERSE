function load(world){
  document.getElementById('portal').style.display='none';
  document.getElementById('current').innerHTML='';
  eval(world+'()'); // cada función carga su mundo completo
  speak('Cargado '+world.toUpperCase()+' · Respirá · Todo está bien');
}

function speak(t){const u=new SpeechSynthesisUtterance(t);u.lang='es-AR';speechSynthesis.speak(u);}
document.addEventListener('keydown',e=>{
  if(e.key==='s') for(let i=0;i<35;i++) {const h=document.createElement('div');h.innerText='❤️';h.style.cssText=`position:fixed;font-size:120px;left:${Math.random()*100}vw;animation:up 3s forwards`;document.body.appendChild(h);setTimeout(()=>h.remove(),3000);}
  if(e.key==='f') {document.body.style.filter='hue-rotate(180deg)';speak('¡OBLIGADOS!');setTimeout(()=>document.body.style.filter='',2000);}
});

