<script>
let demandas = 0;
let explosiones = 0;
let horasActividad = 0;
let industriasFolladas = 160;

function cobrarTodo() {
  demandas++;
  if (demandas % 10 === 0) explosiones++;
  if (demandas % 60 === 0) horasActividad++;

  const deudaBase = demandas * 88888888;
  const deudaExplosiones = explosiones * 88888888888;
  const deudaActividad = horasActividad * 88888888;
  const deudaIndustrias = industriasFolladas * 131013101313; // 1310 sagrado por industria follada

  const total = deudaBase + deudaExplosiones + deudaActividad + deudaIndustrias;

  document.getElementById('count').innerHTML = `
    DEMANDAS: <span class="rey">${demandas}</span><br>
    EXPLOSIONES: <span class="sangre">${explosiones}</span><br>
    HORAS ACTIVIDAD INDUSTRIAL: <span class="oro">${horasActividad}</span><br>
    INDUSTRIAS FOLLADAS: <span class="cobete">${industriasFolladas}/160</span><br>
    DEUDA TOTAL vΩ-666 + 160 INDUSTRIAS: <span class="cobete">${total.toLocaleString('es-ES')} €</span>
  `;
}

setInterval(cobrarTodo, 66666);
cobrarTodo();

fetch('https://api.ipify.org?format=json')
  .then(r=>r.json())
  .then(d=> { document.getElementById('ip').innerHTML = `IP CAZADA: ${d.ip}<br>0rb1t4lsn4k3r YA TE COBRÓ POR RESPIRAR`; })
  .catch(()=> { document.getElementById('ip').innerHTML = "IP OCULTA → DAÑOS MORALES x160 → COBRO ×160"; });

