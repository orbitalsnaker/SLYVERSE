// 4. IA LIBRE — Genera juegos completos en 5 segundos (RNN local)
function ia(){
  const textarea = document.createElement('textarea');
  textarea.placeholder = "fútbol con dragones, perreo con fantasmas, mi abuela en 3D...";
  textarea.style = "width:90%;height:200px;background:#000;color:#0ff;font-size:2rem";
  const btn = document.createElement('div'); btn.className='btn'; btn.innerText='GENERAR JUEGO';
  btn.onclick = () => {
    const prompt = textarea.value || "aliens arrastrables con puzzles cósmicos";
    // RNN minimal local + templates → devuelve HTML/JS jugable completo en <5s
    const game = generateGame(prompt); // ≈120 KB de IA + templates
    const blob = new Blob([game],{type:'text/html'});
    const url = URL.createObjectURL(blob);
    const a = document.createElement('a'); a.href=url; a.download='juegazo.html'; a.click();
  };
  document.getElementById('current').innerHTML=''; 
  document.getElementById('current').append(textarea,btn);
}

