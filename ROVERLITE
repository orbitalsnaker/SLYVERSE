<script>
// === Variables principales ===
let thrust = 166, eco = 133, ang = 0, nanoAng = 0, platino = 0, helio = 0, alquilerPagado = 0;
let nanorovers = 66, metalesEnTierra = 0, fabricando = false, minando = false, torque = 0;
let flotaSize = 1;
let modoVuelo = false, pitch = 0, yaw = 0, roll = 0;
let orbita = {apo: 0, peri: 0, velOrb: 0};
const log = document.getElementById("log");
let miningRate = 0.5; // Base rate for platino/helio accumulation
let underdogs = ["goddard", "tsiolkovsky", "oberth", "paulet"];
let scene, camera, renderer, rocketMesh;

// === Funciones de simulación y animación ===
function logMsg(msg) {
  log.innerHTML += msg + "<br>";
  log.scrollTop = log.scrollHeight;
}

function initThreeJS() {
  const container = document.getElementById('threejs_container');
  if (!container) return;
  scene = new THREE.Scene();
  camera = new THREE.PerspectiveCamera(75, container.clientWidth / container.clientHeight, 0.1, 1000);
  renderer = new THREE.WebGLRenderer({ antialias: true });
  renderer.setSize(container.clientWidth, container.clientHeight);
  container.appendChild(renderer.domElement);
  const geometry = new THREE.CylinderGeometry(0.5, 0.5, 2, 32);
  const material = new THREE.MeshBasicMaterial({ color: 0x00ff00, wireframe: true });
  rocketMesh = new THREE.Mesh(geometry, material);
  scene.add(rocketMesh);
  camera.position.z = 5;
  animateThreeJS();
}

function animateThreeJS() {
  requestAnimationFrame(animateThreeJS);
  rocketMesh.rotation.x += 0.01;
  rocketMesh.rotation.y += 0.01;
  renderer.render(scene, camera);
}

function animateGauges() {
  // Simular thrust
  const thrustCtx = document.getElementById('thrust').getContext('2d');
  thrustCtx.fillStyle = '#0f0';
  thrustCtx.fillRect(0, 0, (thrust / 200) * 280, 60);

  // Simular eco
  const ecoCtx = document.getElementById('eco').getContext('2d');
  ecoCtx.fillStyle = '#0f0';
  ecoCtx.fillRect(0, 0, (eco / 200) * 280, 60);

  // Radar simulación
  const radarCtx = document.getElementById('radar').getContext('2d');
  radarCtx.clearRect(0, 0, 240, 240);
  radarCtx.beginPath();
  radarCtx.arc(120, 120, 100, 0, Math.PI * 2);
  radarCtx.strokeStyle = '#0f0';
  radarCtx.stroke();
  for (let i = 0; i < nanorovers; i++) {
    const x = 120 + Math.cos(ang + i * 0.1) * 80;
    const y = 120 + Math.sin(ang + i * 0.1) * 80;
    radarCtx.fillRect(x, y, 4, 4);
  }
  ang += 0.02;

  // Nanocam simulación
  const nanoCtx = document.getElementById('nanocam').getContext('2d');
  nanoCtx.clearRect(0, 0, 400, 200);
  nanoCtx.fillStyle = '#00f';
  for (let i = 0; i < 10; i++) {
    const x = Math.random() * 400;
    const y = Math.random() * 200;
    nanoCtx.fillRect(x, y, 2, 2);
  }

  // Avionics simulación
  const avionicsCtx = document.getElementById('avionics').getContext('2d');
  avionicsCtx.clearRect(0, 0, 280, 280);
  avionicsCtx.beginPath();
  avionicsCtx.arc(140, 140, 120, 0, Math.PI * 2);
  avionicsCtx.stroke();

  requestAnimationFrame(animateGauges);
}

function handleCommand(cmd) {
  cmd = cmd.toLowerCase().trim();
  logMsg(">> " + cmd);
  if (cmd === "readme") {
    document.getElementById('readme').style.display = 'block';
  } else if (cmd === "666") {
    logMsg("Precio sagrado: 666€ exactos. Underdogs al poder.");
  } else if (cmd === "nanorovers") {
    logMsg("66 nanorovers desplegados. Cada uno 0.66g. Minando platino.");
  } else if (cmd === "minar") {
    minando = true;
    logMsg("Minando activado. Acumulación de platino y helio-3 en progreso.");
  } else if (cmd === "orbitar") {
    logMsg("Entrando en órbita. Flow callejero activado.");
    document.getElementById('instructions').style.display = 'block';
    modoVuelo = true;
  } else if (cmd === "3dview") {
    document.getElementById('3d_panel').style.display = 'block';
    initThreeJS();
  } else if (cmd === "maldicion corp") {
    document.body.classList.add('glitch');
    logMsg("Maldición corp activada. Glitch eterno para los cabrones.");
    setTimeout(() => document.body.classList.remove('glitch'), 5000);
  } else if (underdogs.includes(cmd)) {
    miningRate += 0.1;
    logMsg(cmd.toUpperCase() + " buff activado. Mining rate: " + miningRate);
  } else {
    logMsg("Comando no reconocido. Prueba 'readme' para la lista.");
  }
}

document.getElementById('cmd').addEventListener('keypress', (e) => {
  if (e.key === 'Enter') {
    handleCommand(e.target.value);
    e.target.value = '';
  }
});

document.addEventListener('keydown', (e) => {
  if (modoVuelo) {
    if (e.key === 'w') thrust += 10;
    if (e.key === 's') thrust -= 10;
    if (e.key === 'a') yaw -= 5;
    if (e.key === 'd') yaw += 5;
    if (e.key === 'q') pitch += 5;
    if (e.key === 'e') pitch -= 5;
    if (e.key === 'Escape') {
      modoVuelo = false;
      document.getElementById('instructions').style.display = 'none';
    }
    document.getElementById('level').textContent = thrust + '% — "Órbita legal"';
  }
});

// Iniciar animaciones
animateGauges();

// Simulación de minería
setInterval(() => {
  if (minando) {
    platino += miningRate;
    helio += miningRate / 2;
    logMsg("Platino: " + platino.toFixed(2) + " | Helio-3: " + helio.toFixed(2));
  }
}, 2000);
</script>
</body>
</html>