<script>
// ABRAZOS + GRIND TDAH
let hugs = 777848, hashes = 0, level = 1;
setInterval(()=>{ hugs++; document.getElementById('hugs').textContent = hugs; },7000);
setInterval(()=>{ hashes += 7; document.getElementById('hash').textContent = hashes.toLocaleString(); },1000);
document.body.addEventListener('click',()=>{ 
  hashes += 150; document.getElementById('hash').textContent = hashes.toLocaleString();
  if(hashes > level*9000){ level++; document.getElementById('lvl').textContent = level; exoGroup.scale.multiplyScalar(1.08); }
});

// CÁMARA TRASERA
navigator.mediaDevices.getUserMedia({ video: { facingMode: "environment" }})
  .then(s=>{ document.getElementById('video').srcObject = s; });

// THREE.JS AR 3D
const renderer = new THREE.WebGLRenderer({antialias:true,alpha:true});
renderer.setSize(innerWidth,innerHeight); renderer.setPixelRatio(devicePixelRatio);
document.getElementById('three').appendChild(renderer.domElement);

const scene = new THREE.Scene();
const camera = new THREE.PerspectiveCamera(60,innerWidth/innerHeight,0.1,100);
camera.position.z = 4.5;

scene.add(new THREE.AmbientLight(0x0a3d0a,9));
scene.add(new THREE.PointLight(0x0a3d0a,15,20));

const exo = new THREE.Group(); scene.add(exo);
const knot = new THREE.Mesh(
  new THREE.TorusKnotGeometry(0.9,0.28,240,20,2,3),
  new THREE.MeshPhongMaterial({color:0x0a3d0a,emissive:0x0a3d0a,emissiveIntensity:1.3})
);
knot.position.y=1.4; exo.add(knot);

const particles = new THREE.Points(
  new THREE.BufferGeometry(),
  new THREE.PointsMaterial({color:0x00ff00,size:0.08,transparent:true,opacity:0.8})
);
const pos = new Float32Array(15000*3);
for(let i=0;i<pos.length;i++) pos[i]=(Math.random()-0.5)*10;
particles.geometry.setAttribute('position',new THREE.BufferAttribute(pos,3));
exo.add(particles);

function animate(){
  const t=Date.now()*0.0005;
  knot.rotation.x=t*0.9; knot.rotation.y=t*1.4;
  exo.rotation.y=Math.sin(t*0.6)*0.15;
  particles.rotation.y=t*0.2;
  renderer.render(scene,camera);
  requestAnimationFrame(animate);
}
animate();

// THUAMER JUEGOS SIMPLIFICADOS (ejemplos reales)
function juego(tipo){
  alert(`¡${tipo} CARGADO EN 3D! (próximamente full versión)`);
  // aquí irían los 26 juegos completos como en el original
}

// IA LIBRE QUE CREA JUEGOS
function crearJuego(){
  const p = document.getElementById('prompt').value || "abrazo eterno";
  const codigo = `<!DOCTYPE html><html><head><meta charset="utf-8"><title>${p}</title>
<style>body{margin:0;background:#000;overflow:hidden}</style></head><body><canvas id=c></canvas>
<script src="https://cdn.jsdelivr.net/npm/three@0.167.0/build/three.min.js"></script>
<script>const s=new THREE.Scene(); c=new THREE.PerspectiveCamera(75,innerWidth/innerHeight,0.1,1000);
r=new THREE.WebGLRenderer({canvas:document.getElementById('c'),alpha:true}); r.setSize(innerWidth,innerHeight);
const g=new THREE.TorusKnotGeometry(1,0.3,160,16); const m=new THREE.MeshBasicMaterial({color:0x0f0,wireframe:true});
const k=new THREE.Mesh(g,m); s.add(k); c.position.z=5; function a(){k.rotation.x+=0.01;k.rotation.y+=0.013;r.render(s,c);requestAnimationFrame(a);}a();<\/script></body></html>`;
  document.getElementById('codigo').textContent = codigo;
}

// OMNITOOL SANACIÓN
const audios = {
  coma:"https://cdn.jsdelivr.net/gh/liminalx/theta-rain@master/theta-4.5hz-432hz-rain-60min.mp3",
  shipibo:"https://cdn.jsdelivr.net/gh/ancestral-sounds/shipibo-ikaros@master/rain-ikaro.mp3",
  mapuche:"https://cdn.jsdelivr.net/gh/ancestral-sounds/mapuche@master/kultrun-viento.mp3",
  guarani:"https://cdn.jsdelivr.net/gh/ancestral-sounds/guarani@master/mboyere.mp3",
  ayahuasca:"https://cdn.jsdelivr.net/gh/ancestral-sounds/ayahuasca@master/icaros-geometry.mp3"
};
function cambiarLinaje(){ document.getElementById('binaural').src=audios[document.getElementById('linaje').value]||audios.coma; document.getElementById('binaural').play(); }
function modoPresencia(){ setTimeout(crearObra,10000); }
function modoRespiracion(){ setTimeout(crearObra,18000); }
function crearObra(){
  hugs++; localStorage.hugs=hugs;
  document.getElementById('output').innerHTML=`<p style="font-size:5rem">OBRA SANADORA CREADA</p><p>Abrazo número ${hugs}<br>La Boa te abraza aunque estés en coma.</p>`;
}

// TOGGLES
function togglePortal(){ document.getElementById('portal').style.display=document.getElementById('portal').style.display==='flex'?'none':'flex'; }
function toggleIA(){ document.getElementById('ia').style.display=document.getElementById('ia').style.display==='flex'?'none':'flex'; }
function toggleOmni(){ document.getElementById('omni').style.display=document.getElementById('omni').style.display==='flex'?'none':'flex'; cambiarLinaje(); }
function toggleReadme(){ document.getElementById('readme').style.display='flex'; }

// RESIZE
window.addEventListener('resize',()=>{ camera.aspect=innerWidth/innerHeight; camera.updateProjectionMatrix(); renderer.setSize(innerWidth,innerHeight); });

function despertar(){
  console.clear();
  console.log("%cFUSIÓN SUPREMA CARGADA · EXO + THUAMER + OMNITOOL + TDAH GRIND · ABRAZO "+hugs,"color:#0a3d0a;font-size:28px");
}
despertar();
</script>
