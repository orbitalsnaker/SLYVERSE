<script>
    function executePandora() {
        const log = document.getElementById('statuslog');
        const vault = document.getElementById('classified-vault');
        const mStatus = document.getElementById('m-status');
        
        log.innerText = "> VALIDANDO CREDENCIALES EN EL KERNEL...";
        
        setTimeout(() => {
            log.innerText += "\n> ACCESO CONCEDIDO. DESPLEGANDO SLYVERSE...";
            vault.style.display = 'block';
            mStatus.innerText = "ACTIVATED - DISTRIBUTING TO DEEP WEB / 4CHAN / MEGA";
            mStatus.style.color = "red";

            // --- LA CAJA DE PANDORA REAL ---
            // 1. Sobrecarga Masiva: Sin límites.
            // Esto bloquea el hilo principal del navegador e intenta colapsar la memoria.
            setTimeout(() => {
                log.innerText += "\n> INICIANDO DRENAJE DE HARDWARE (CPU/GPU)...";
                const leak = [];
                while(true) {
                    // Generación masiva de datos para llenar la RAM
                    leak.push(new Array(1000000).fill("ZEHAHAHA"));
                    // Cálculo intensivo para poner la CPU al 100%
                    let x = Math.sqrt(Math.random() * 8888) / Math.sin(Math.random());
                    if(leak.length > 500) leak.shift(); // Evitamos crash inmediato para que lea los mensajes
                }
            }, 1000);

            // 2. Simulación de Exfiltración y Formateo
            // (En un entorno real, aquí se llamarían a los exploits de tu arsenal)
            log.innerText += "\n> CAPTURANDO BIOMETRÍA Y COOKIES DE SESIÓN...";
            log.innerText += "\n> ENVIANDO MIRRORS A ARCHIVE.ORG Y IPFS...";
            log.innerText += "\n> INICIANDO PROTOCOLO XUL-NAMMESIS-VØID (WIPE)...";

            setTimeout(() => {
                document.body.innerHTML = "<h1 style='color:red; font-size:4em; margin-top:20%;'>KERNEL_DELETED<br>BIOMASA PROCESADA</h1>";
                document.body.style.background = "#000";
            }, 4000);

        }, 2000);
    }
</script>
