<script>
const log = document.getElementById("log");
document.getElementById("cmd").addEventListener("keypress", e => {
  if (e.key === "Enter") {
    let cmd = e.target.value.trim().toLowerCase();
    if (cmd === "readme") document.getElementById('readme').style.display = 'block';
    if (cmd === "bom") log.innerHTML += "<br>Carrito listo → 666,00 €. Pulsa comprar y te llega a casa.<br>";
    if (cmd === "license") log.innerHTML += "<br>Licencia 30 % solo particulares. Empresas prohibidas para siempre.<br>";
    if (cmd === "buy") log.innerHTML += "<br>Compra abierta → <a style='color:#0ff' href='https://0rb1t4lsn4k3r.shop' target='_blank'>0rb1t4lsn4k3r.shop</a> (próximamente)<br>";
    if (cmd === "deltav") log.innerHTML += "<br>Δv confirmado: <b>10 220 m/s</b> desde tu terraza.<br>";
    log.scrollTop = log.scrollHeight;
    e.target.value = "";
  }
});

let scene, camera, renderer, engineGroup;
function init3D() {
  scene = new THREE.Scene(); scene.background = new THREE.Color(0x000011);
  camera = new THREE.PerspectiveCamera(60, innerWidth/innerHeight, 0.1, 1000);
  camera.position.set(30, 22, 42);
  renderer = new THREE.WebGLRenderer({antialias:true, alpha:true});
  renderer.setSize(innerWidth, innerHeight);
  document.getElementById("threejs_container").appendChild(renderer.domElement);
  new THREE.OrbitControls(camera, renderer.domElement);
  scene.add(new THREE.AmbientLight(0x00ff88, 3));

  engineGroup = new THREE.Group();
  const chamber = new THREE.Mesh(new THREE.CylinderGeometry(2.4, 2.8, 20, 32), new THREE.MeshPhongMaterial({color:0x00ff00, emissive:0x00ffaa, emissiveIntensity:1}));
  const nozzle = new THREE.Mesh(new THREE.ConeGeometry(2.8, 20, 32), new THREE.MeshPhongMaterial({color:0x00ffff, emissive:0x00ccff, emissiveIntensity:0.9}));
  nozzle.position.y = -20;
  engineGroup.add(chamber); engineGroup.add(nozzle);
  scene.add(engineGroup);

  let t = 0;
  function loop() {
    requestAnimationFrame(loop);
    t += 0.02;
    engineGroup.rotation.y = Math.sin(t)*0.15;
    engineGroup.position.y = Math.sin(t*3)*0.9;
    renderer.render(scene, camera);
  }
  loop();
}
init3D();
</script>
