  c.strokeStyle="#0f0";c.lineWidth=4;c.beginPath();
  c.moveTo(190,140);c.lineTo(190+50*Math.cos(brazo*1.3),140+50*Math.sin(brazo*1.3));c.stroke();
  brazo+=0.18;

  const m=document.getElementById("mine").getContext("2d");
  const prog=(Date.now()/50)%100;
  if(Math.random()>0.85){platino+=1; if(Math.random()>0.7)helio+=0.5;}
  m.fillStyle="#000";m.fillRect(0,0,380,60);
  m.fillStyle="#ff0";m.fillRect(0,0,prog*3.8,60);
  document.getElementById("yield").textContent=`Platino: \( {platino}kg — Helio-3: \){helio.toFixed(1)}g — Pa los míos del 667`;
},120);

