  {init(){this.x=150;this.y=h-150;this.vx=0;this.g=0;this.score=0;this.obs=[];this.high=+localStorage.getItem('hs0')||0;document.getElementById('hs').textContent=this.high},
   update(){
     c.width=w;c.height=h;ctx.fillStyle='#000';ctx.fillRect(0,0,w,h);
