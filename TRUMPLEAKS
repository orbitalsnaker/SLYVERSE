            const r = rebellionData[Math.floor(Math.random()*rebellionData.length)];
            const rDiv = document.createElement('div');
            rDiv.innerHTML = `<span style="color:var(--gold);">[REBEL]</span> ${r}`;
            document.getElementById('rebel-logs').prepend(rDiv);

            // Contador de Mirrors
            let mc = parseInt(document.getElementById('mirror-count').innerText.replace(/\./g, ''));
            document.getElementById('mirror-count').innerText = (mc + 15).toLocaleString('es-ES');
        }, 1000);
    }

    window.addEventListener("beforeunload", () => {
