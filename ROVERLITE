<script>
const log = document.getElementById("log");
document.getElementById("cmd").addEventListener("keypress", e => {
  if (e.key === "Enter") {
    let cmd = e.target.value.trim().toLowerCase();
    if (cmd === "readme") document.getElementById('readme').style.display = 'block';
    if (cmd === "bom") log.innerHTML += "<br>BOM demoníaco listo → 666,00 €. El infierno entrega en 48 h.<br>";
    if (cmd === "license") log.innerHTML += "<br>Licencia 30 %. Empresas = infierno jurídico eterno.<br>";
    if (cmd === "demon") log.innerHTML += "<br>DEMON CORE v2: 272 s Isp · 34 kN · 5 100 m/s · sobrevives (probabilidad 99,7 %)<br>";
    if (cmd === "buy") log.innerHTML += "<br>Compra abierta → <a style='color:#ff0066' href='https://0rb1t4lsn4k3r.shop' target='_blank'>0rb1t4lsn4k3r.shop</a> (abriendo el portal pronto)<br>";
    log.scrollTop = log.scrollHeight;
    e.target.value = "";
  }
});

let scene, camera, renderer, engineGroup;
function init3D() {
  scene = new THREE.Scene(); scene.background = new THREE.Color(0x110000);
  camera = new THREE.PerspectiveCamera(60, innerWidth/innerHeight, 0.1, 1000);
  camera.position.set(35, 25, 50);
  renderer = new THREE.WebGLRenderer({antialias:true, alpha:true});
  renderer.setSize(innerWidth, innerHeight);
  document.getElementById("threejs_container").appendChild(renderer.domElement);
  new THREE.OrbitControls(camera, renderer.domElement);
  scene.add(new THREE.AmbientLight(0xff0066, 2));
  scene.add(new THREE.PointLight(0xff0066, 10, 100));

  engineGroup = new THREE.Group();
  
  // Cámara con camisa cerámica roja infernal
  const chamber = new THREE.Mesh(
    new THREE.CylinderGeometry(2.6, 3.0, 22, 32),
    new THREE.MeshPhongMaterial({color:0xff0033, emissive:0xff0066, emissiveIntensity:2})
  );
  
  // Tobera con brillo demoníaco
  const nozzle = new THREE.Mesh(
    new THREE.ConeGeometry(3.0, 24, 32),
    new THREE.MeshPhongMaterial({color:0xff0000, emissive:0xff3366, emissiveIntensity:2, metalness:0.9})
  );
  nozzle.position.y = -23;

  // Llama viva
  const flameGeo = new THREE.ConeGeometry(4, 30, 32, 8, true);
  flameGeo.translate(0, -30, 0);
  const flame = new THREE.Mesh(flameGeo, new THREE.MeshBasicMaterial({
    color:0xff0066, transparent:true, opacity:0.8, side:THREE.DoubleSide
  }));

  engineGroup.add(chamber, nozzle, flame);
  scene.add(engineGroup);

  let t = 0;
  function loop() {
    requestAnimationFrame(loop);
    t += 0.03;
    engineGroup.rotation.y = Math.sin(t)*0.2;
    engineGroup.position.y = Math.sin(t*4)*1.2;
    flame.scale.y = 0.8 + Math.sin(t*20)*0.3;
    flame.material.opacity = 0.6 + Math.sin(t*15)*0.2;
    renderer.render(scene, camera);
  }
  loop();
}
init3D();
</script>
