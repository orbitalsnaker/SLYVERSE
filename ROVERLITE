<script>
const log = document.getElementById("log");
document.getElementById("cmd").addEventListener("keypress", e => {
  if (e.key === "Enter") {
    let cmd = e.target.value.trim().toLowerCase();
    if (cmd === "readme") document.getElementById('readme').style.display = 'block';
    if (cmd === "patent") log.innerHTML += "<br><span style='color:#ff0'>Nueva patente registrada ES-2025-666-0rb1t4lsn4k3r-v19: “Motor híbrido catalítico de ignición instantánea”</span><br>";
    if (cmd === "engine") log.innerHTML += "<br>AIRWEAVE ENGINE v15 → 28 kN | Isp 312 s | 100 % seguro | patentado<br>";
    if (cmd === "dia1") log.innerHTML += "<br>2026-06-21 → Madrileño-1 aterriza. Horno solar OK. Primer ladrillo lunar impreso en 4 h 12 min.<br>";
    if (cmd === "platino") log.innerHTML += "<br>2026-06-29 → 1 kg platino puro en Madrid. Valor calle: 1.020.000 €. Beneficio neto: +1.019.334 €.<br>";
    log.scrollTop = log.scrollHeight;
    e.target.value = "";
  }
});

// 3D motor AIRWEAVE
let scene, camera, renderer, engine;
function init3D() {
  scene = new THREE.Scene(); scene.background = new THREE.Color(0x000011);
  camera = new THREE.PerspectiveCamera(60, innerWidth/innerHeight, 0.1, 1000);
  camera.position.set(20, 15, 30);
  renderer = new THREE.WebGLRenderer({antialias:true, alpha:true});
  renderer.setSize(innerWidth, innerHeight);
  document.getElementById("threejs_container").appendChild(renderer.domElement);
  new THREE.OrbitControls(camera, renderer.domElement);

  scene.add(new THREE.AmbientLight(0x00ff88, 2));

  engine = new THREE.Group();
  const chamber = new THREE.Mesh(new THREE.CylinderGeometry(2, 2.5, 12, 32), new THREE.MeshPhongMaterial({color:0x00ff00, emissive:0x00ff00, emissiveIntensity:0.6}));
  const nozzle = new THREE.Mesh(new THREE.ConeGeometry(2.5, 8, 32), new THREE.MeshPhongMaterial({color:0x00ffff, emissive:0x0088ff}));
  nozzle.position.y = -10;
  engine.add(chamber); engine.add(nozzle);
  scene.add(engine);

  let t = 0;
  function loop() {
    requestAnimationFrame(loop);
    t += 0.02;
    engine.rotation.y = Math.sin(t)*0.1;
    engine.position.y = Math.sin(t*3)*0.5;
    renderer.render(scene, camera);
  }
  loop();
}
init3D();
</script>
