<script>
    function setLanguage(lang) {
        document.querySelectorAll('[data-en],[data-ja]').forEach(el => {
            el.textContent = el.getAttribute(`data-${lang}`);
        });
        document.documentElement.lang = lang;
        document.querySelectorAll('.lang-toggle button').forEach(btn => {
            btn.classList.toggle('active', btn.getAttribute('onclick') === `setLanguage('${lang}')`);
        });
    }

    // Auto-detect browser language on load
    window.onload = function() {
        const browserLang = (navigator.language || navigator.userLanguage).substring(0, 2);
        const supported = ['en', 'ja'];
        const lang = supported.includes(browserLang) ? browserLang : 'en';
        setLanguage(lang);
    };
</script>

