<script>
// 99 nanorovers, todo más rápido, más bestia, mismo precio
let nanorovers=99, minando=false;
const log=document.getElementById("log");

document.getElementById("cmd").addEventListener("keypress",e=>{
  if(e.key==="Enter"){
    let cmd=e.target.value.toLowerCase().trim();
    if(cmd==="final"||cmd==="readme") document.getElementById('readme').style.display='block';
    if(cmd==="minar"){minando=true;log.innerHTML+="<br>99 nanorovers minando a 400 s Isp<br>";}
  }
});

setInterval(()=>{if(minando){
  log.innerHTML+="+9.99 g Platino | +6.66 g Helio-3 → 70 % cae al barrio automáticamente<br>";
  log.scrollTop=log.scrollHeight;
}},1400);

// 3D con 99 nanorovers y carbono brillante
let scene,camera,renderer,rocket,nanos=[];
function init3D(){
  scene=new THREE.Scene(); scene.background=new THREE.Color(0x000000);
  camera=new THREE.PerspectiveCamera(60,innerWidth/innerHeight,0.1,1000); camera.position.set(18,14,24);
  renderer=new THREE.WebGLRenderer({antialias:true,alpha:true}); renderer.setSize(innerWidth,innerHeight);
  document.getElementById("threejs_container").appendChild(renderer.domElement);
  new THREE.OrbitControls(camera,renderer.domElement);
  scene.add(new THREE.AmbientLight(0x00ff00,2));

  rocket=new THREE.Group();
  const cuerpo=new THREE.Mesh(new THREE.CylinderGeometry(1.2,1.2,7,64),new THREE.MeshPhongMaterial({color:0x00ff00,emissive:0x00ff00,shininess:100}));
  rocket.add(cuerpo); scene.add(rocket);

  for(let i=0;i<99;i++){
    const n=new THREE.Mesh(new THREE.SphereGeometry(0.1,20,20),new THREE.MeshPhongMaterial({color:0x00ffff,emissive:0x00ff00}));
    nanos.push(n); scene.add(n);
  }

  let ang=0;
  function loop(){
    requestAnimationFrame(loop);
    rocket.rotation.y+=0.003;
    nanos.forEach((n,i)=>{const a=ang+i*0.064;n.position.set(Math.cos(a)*8,Math.sin(a*1.7)*4,Math.sin(a)*8);});
    ang+=0.02;
    renderer.render(scene,camera);
  }
  loop();
}
init3D();
</script>
