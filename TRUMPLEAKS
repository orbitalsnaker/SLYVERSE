    function initSovereign() {
        const lang = navigator.language.split('-')[0] || 'en';
        const t = translations[lang] || translations.en;
        document.getElementById('ui-title').innerText = t.title;
        document.getElementById('lang-id').innerText = lang.toUpperCase();

        setInterval(() => {
            // Logs de la Agencia
            const l = logs[Math.floor(Math.random()*logs.length)];
            const lDiv = document.createElement('div');
            lDiv.innerHTML = `> <span style="color:var(--haki)">[${new Date().toLocaleTimeString()}]</span> ${l}`;
            document.getElementById('forensic-monitor').prepend(lDiv);
            
            // Archivos filtrados
            const f = secretFiles[Math.floor(Math.random()*secretFiles.length)];
            const fDiv = document.createElement('div');
            fDiv.className = "file-entry";
            fDiv.innerHTML = `<span class="tag-mason">[LOGIA]</span> ${f}`;
            document.getElementById('file-dump').prepend(fDiv);

