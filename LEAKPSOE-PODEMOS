    function startDrenaje() {
        const list = document.getElementById('list-target');
        const corruptos = [
            "F. GONZÁLEZ (SEÑOR X) - NODO: FUNDADOR_GAL",
            "P. SÁNCHEZ (SEPULTURERO) - NODO: MONCLOA_CORE",
            "Y. DÍAZ (MODISTA) - NODO: COMUNISMO_LAVADO",
            "F. MARLASKA (CARCELERO) - NODO: BARAJAS_DELCY",
            "J.C. MONEDERO (CAJERO) - NODO: CARTEL_SOLES",
            "J.L.R. ZAPATERO (MEDIADOR) - NODO: ORO_PDVSA",
            "P. IGLESIAS (ACTIVO_P) - NODO: FINANC_IRAN_VEN",
            "M.A. MORATINOS (LOGIA) - NODO: EXPANSIÓN_B1",
            "B. BEGOÑA (SECRET_B) - NODO: CÁTEDRA_TRÁFICO",
            "S. ILLA (QUÍMICO) - NODO: MÁSCARAS_FALSAS",
            "KOLDO G. (LOGÍSTICO) - NODO: PUTICLUB_MANDO",
            "V. ALDAMA (HIDRO) - NODO: CARTEL_HIDROCARBUROS"
        ];

        // GENERACIÓN DE 1000 LÍNEAS DE EVIDENCIA
        for(let i=1; i<=1000; i++) {
            let entry = document.createElement('div');
            let name = corruptos[i % corruptos.length];
            entry.innerHTML = `<span style="color:var(--gold)">[LÍNEA_${i}]</span> <span style="color:#555">UID_${Math.random().toString(36).substr(2, 9)}</span>: ${name} | STATUS: <span class="critical">TARGET_FOR_EXTRADITION</span> | LOG: DRENAJE_COMPLETE`;
            list.appendChild(entry);
            
            // Auto-scroll para efecto terminal
            if(i > 20) {
                document.getElementById('scrolling-data').scrollTop = document.getElementById('scrolling-data').scrollHeight;
            }
        }
        console.log("KERNEL RONIN: 1000 LÍNEAS DE CORRUPCIÓN INDEXADAS. MONCLOA OFFLINE.");
    }
</script>

