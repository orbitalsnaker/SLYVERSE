function sendTribute() {
  navigator.clipboard.writeText('0xfddb91bd8157dafb735d4087cff8823b42eaadc7').then(() => {
    alert('¡Tributo registrado en el Stripclub Eterno!\n0xfddb91bd8157dafb735d4087cff8823b42eaadc7\n\nLa Diosa gira en el pole y ríe. Vive largo y sigue pagando. 666');
  }).catch(() => {
    prompt('Copia el portal del cáliz:', '0xfddb91bd8157dafb735d4087cff8823b42eaadc7');
  });
}
