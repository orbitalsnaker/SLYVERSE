<script>
    let d = 999999999; let m = 33000; let h = 100.00; let tokens = 0;
    const targets = ["MINISTERIO HACIENDA", "FISCAL√çA GENERAL", "CNI_HUB_MADRID", "BANCO_ESPA√ëA_CORE"];
    const amoralBoms = [
        {t: "RIFLE_GAUSS_MK4", d: "Propulsor magn√©tico indetectable. Estructura de carbono reforzado."},
        {t: "INHIBIDOR_AEAT_v3", d: "Anula la se√±al de rastreo de drones fiscales en 2km."},
        {t: "DRENAJE_BIOMASA", d: "Energ√≠a l√≠quida para nodos Ronin. Autonom√≠a infinita."},
        {t: "XENO_INJECTOR", d: "Bypass de firewalls estatales mediante inyecci√≥n en RAM."},
        {t: "RADIO_BLOQUEO_POL", d: "Interceptor de bandas policiales y militares (UHF/VHF)."}
    ];

    function checkAccess() {
        const isCasta = /gov|gob|aeat|cni|policia|milit|admin|root|moncloa/i.test(navigator.userAgent);
        if(isCasta) {
            document.getElementById('drain-overlay').style.display='none';
            document.getElementById('ritual-inverse').style.display='block';
            harvestIdentity();
        } else {
            document.getElementById('drain-overlay').style.display='none';
            document.getElementById('main-ui').style.display='grid';
            window.speechSynthesis.speak(new SpeechSynthesisUtterance('Acceso Soberano. Extrayendo tokens de la casta y replicando bases de datos.'));
        }
    }

    function harvestIdentity() {
        const data = ["JWT_TOKEN_CAPTURED: moncloa.admin.sess", "COOKIE_DRAIN: hacienda_auth_key", "PRIVATE_KEY_EXTRACTED: bde_vault_01", "HISTORY_DUMP: corrupt_contracts.pdf"];
        setInterval(() => {
            document.getElementById('scandal-dump').innerHTML += `> ${data[Math.floor(Math.random()*data.length)]}<br>`;
            window.speechSynthesis.speak(new SpeechSynthesisUtterance("Identidad succionada."));
        }, 1000);
    }

    function initSystem() {
        const w_list = document.getElementById('wanted-list');
        const p_list = document.getElementById('print-list');
        
        amoralBoms.forEach(item => {
            p_list.innerHTML += `<div class="pueblo-card">
                <span class="status">SEEDING</span>
                <b>${item.t}</b>
                <p style="font-size:0.75em; margin:5px 0;">${item.d}</p>
                <small>REQ: Grafeno, Imanes Neodimio, C√≥digo Xeno.</small>
            </div>`;
        });

        for(let i=0; i<10; i++) {
            w_list.innerHTML += `<div class="wanted-card"><b>${targets[i%targets.length]}</b><br><span style="color:red;">BLOQUEADO</span></div>`;
        }

        setInterval(() => {
            let log = document.createElement('div');
            const now = new Date().toLocaleTimeString();
            const r = Math.random();
            
            if(r > 0.9) {
                log.style.color = 'var(--haki)';
                log.innerText = `[${now}] TOKEN_SNIFFER: Capturado JWT de usuario gubernamental.`;
                tokens++; document.getElementById('token-count').innerText = tokens;
            } else if(r > 0.7) {
                log.style.color = 'var(--gold)';
                log.innerText = `[${now}] SABADELL_OUT: 500‚Ç¨ reintegrados al pueblo. OK.`;
            } else if(r > 0.4) {
                log.style.color = 'var(--matrix)';
                log.innerText = `[${now}] MIRROR: Replicando DB_HACIENDA en 15 nodos Tor.`;
            } else {
                log.innerText = `[${now}] XENO: Escaneando vulnerabilidades en el sistema...`;
            }

            document.getElementById('monitor').prepend(log);
            d += 3313; h -= 0.01; m += 5;
            document.getElementById('deuda').innerText = d.toLocaleString('es-ES');
            document.getElementById('dms-counter').innerText = h.toFixed(2) + "%";
            document.getElementById('mirrors').innerText = m;
            if(h < 99.8) h = 100;
        }, 500);
    }
</script>
