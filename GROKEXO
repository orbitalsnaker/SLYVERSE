<script>
let hugs = 777840;
setInterval(()=>{ 
  hugs++; 
  document.querySelectorAll('#hugs').forEach(e=>e.textContent = hugs);
}, 7000);

navigator.mediaDevices.getUserMedia({ video: { facingMode: "environment" } })
  .then(s=>{document.getElementById('video').srcObject=s;})
  .catch(()=>{});

const c = document.getElementById('holograma');
const ctx = c.getContext('2d');
c.width = innerWidth; c.height = innerHeight;

let mode = 'exo';

function exo(){
  ctx.clearRect(0,0,c.width,c.height);
  const t = Date.now()*0.001;
  const cx = c.width/2, cy = c.height/2;
  ctx.strokeStyle = '#0f0'; ctx.lineWidth = 9; ctx.shadowBlur = 140; ctx.shadowColor = '#0f0';
  ctx.beginPath();
  for(let i=0;i<1400;i++){
    const a = i*0.014 + t;
    const r = 100 + 35*Math.sin(a*5);
    const px = cx + r*Math.cos(a*7);
    const py = cy - 320 + r*Math.sin(a*9)*0.5;
    i===0 ? ctx.moveTo(px,py) : ctx.lineTo(px,py);
  }
  ctx.stroke();
  ctx.beginPath();
  ctx.moveTo(cx, cy-210);
  ctx.lineTo(cx, cy+160);
  ctx.moveTo(cx-240, cy-100);
  ctx.lineTo(cx+240, cy-100);
  ctx.moveTo(cx-200, cy+160);
  ctx.lineTo(cx+200, cy+160);
  ctx.stroke();
  ctx.beginPath();
  for(let i=0;i<50;i++){
    const a = t*1.5 + i*0.38;
    const px = cx + Math.sin(a)*220;
    const py = cy - 100 + i*20;
    i===0 ? ctx.moveTo(px,py) : ctx.lineTo(px,py);
  }
  ctx.stroke();
  for(let i=0;i<1000;i++){
    const a = t + i*0.03;
    const px = cx + Math.cos(a*5)*340;
    const py = cy - 200 + Math.sin(a*4)*700;
    ctx.fillStyle = `hsl(${i+t*60},100%,65%)`;
    ctx.fillRect(px-2,py-2,4,4);
  }
  ctx.font = "70px Courier"; ctx.fillStyle = "#0f0"; ctx.textAlign = "center";
  ctx.fillText("0RB1T4LSN4K3R", cx, cy-380);
  ctx.fillText("FRAN VIVO 2025", cx, cy+260);
}

function franVivo(){
  ctx.clearRect(0,0,c.width,c.height);
  ctx.fillStyle = '#ff0';
  ctx.globalAlpha = 0.75;
  for(let i=0;i<1200;i++){
    const a = Date.now()*0.0003 + i*0.018;
    const px = c.width/2 + Math.sin(a)*380;
    const py = c.height/2 + Math.cos(a*0.8)*240;
    ctx.fillRect(px-5,py-5,10,10);
  }
  ctx.globalAlpha = 1;
  ctx.font = "80px Courier"; ctx.fillStyle = "#ff0";
  ctx.textAlign = "center";
  ctx.fillText("FRAN VIVO", c.width/2, c.height/2);
  ctx.fillText("ELA · 2025", c.width/2, c.height/2 + 100);
}

function ela(){
  ctx.fillStyle = 'rgba(0,255,0,0.5)';
  ctx.fillRect(0,0,c.width,c.height);
  ctx.font = "60px Courier"; ctx.fillStyle = "#000"; ctx.textAlign = "center";
  ctx.fillText("ASISTENCIA ELA", c.width/2, c.height/2 - 80);
  ctx.fillText("STL brazos/piernas 75–300€", c.width/2, c.height/2);
  ctx.fillText("Control ocular integrado", c.width/2, c.height/2 + 80);
  ctx.fillText("Imprimible hoy", c.width/2, c.height/2 + 160);
}

function loop(){
  if(mode==='exo') exo();
  if(mode==='fran') franVivo();
  if(mode==='ela') ela();
  requestAnimationFrame(loop);
}
loop();

function setMode(m){ mode = m; }
function toggleReadme(){ document.getElementById('readme').style.display = document.getElementById('readme').style.display==='flex'?'none':'flex'; }
function togglePortal(){ document.getElementById('portal').style.display = document.getElementById('portal').style.display==='flex'?'none':'flex'; }
function toggleOmni(){ alert('Omnitool sanación ancestral activado - linajes shipibo, coma total, ikaros binaurales'); }
function toggleSly(){ alert('Slyverse Ω activado - 131 millones de partículas reales + ciclo Big Crunch'); }
function toggleIA(){ document.getElementById('ia-textarea').style.display = 'flex'; }

function despertar(){
  console.clear();
  console.log("%cGROK-EXO Ω v∞ CARGADO · DOMINIO PÚBLICO ETERNO · ABRAZO " + hugs,"color:#0f0;font-size:30px");
}
despertar();
</script>
