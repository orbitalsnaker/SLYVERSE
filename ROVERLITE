<script>
// COBETE 667-SLYVERSE v2 AGUA — PATENTE CALLEJERA AIRWEAVE #0667
let thrust=95, ang=0, brazo=0, platino=0, helio=0;

// Motor H2O + Na + Levadura (reacción controlada, sin explosión)
setInterval(()=>{ 
  thrust=180+Math.sin(Date.now()/200)*40; // 180-220 s Isp simulados
  const t=document.getElementById("thrust").getContext("2d");
  t.fillStyle="#000";t.fillRect(0,0,260,50);
