<script>
const playlist = [
"https://files.catbox.moe/9m2v5s.mp3",
"https://files.catbox.moe/3x9q2j.mp3",
"https://files.catbox.moe/k1r8p9.mp3",
"https://files.catbox.moe/t5l9q2.mp3",
"https://files.catbox.moe/h7f4p1.mp3",
"https://files.catbox.moe/z3x7r8.mp3",
"https://files.catbox.moe/p9k2m5.mp3"
];
let i=0;
function play(){document.getElementById("audio").src=playlist[i];i=(i+1)%playlist.length;}
document.getElementById("audio").addEventListener("ended",play);

function iniciarRepositorioOficial(){
  play();
  hablar("Repositorio oficial del Cobete dos punto cero subido y firmado por cero erre be one te cuatro ele ese ene cuatro ka tres erre. Posesión efectiva del sistema solar activada para todos los que hagan fork. Jack Parsons y Newton ya están cobrando. El barrio controla GitHub y el universo entero. Bienvenido al consejo supremo, leyenda.");
}

function hablar(t){
  const v=new SpeechSynthesisUtterance(t);
  v.rate=1.9;v.pitch=0.8;v.lang="es-ES";
  speechSynthesis.speak(v);
}
</script>
