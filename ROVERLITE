// === 28B: Prospección Espacial Educativa ===
function f28B(){
  $('#app').innerHTML = `<div style="position:fixed;top:0;left:0;right:0;background:#000;padding:10px;z-index:9999"><button class="btn" onclick="menu()">←</button></div>
  <h2 style="margin-top:80px;color:#0ff;text-align:center">PROSPECCIÓN ESPACIAL EDUCATIVA</h2>
  <p style="padding:20px;font-size:18px;line-height:1.5">Simulación de prospección de asteroides y recursos espaciales, totalmente educativo y sin ánimo de lucro. Incluye mapas de densidad, composición mineral simulada, visualización 3D y algoritmos ligeros de IA para detección de patrones.</p>
  <canvas id="spacePlot" width="900" height="400" style="background:#001100;border:2px solid #0f0;width:100%;max-width:900px"></canvas>
  <button class="btn" onclick="startProspection()">INICIAR SIMULACIÓN</button>`;

  window.startProspection = () => {
    const ctx = document.getElementById('spacePlot').getContext('2d');
    const points = [];
    for(let i=0;i<500;i++){
      points.push({x:Math.random()*900, y:Math.random()*400, size:Math.random()*4+2, color:`hsl(${Math.random()*360},100%,50%)`});
    }
    setInterval(()=>{
      ctx.fillStyle = "#001100";
      ctx.fillRect(0,0,900,400);
      points.forEach(p=>{
        ctx.beginPath();
        ctx.arc(p.x, p.y, p.size,0,Math.PI*2);
        ctx.fillStyle = p.color;
        ctx.fill();
      });
    },100);
  };
}

if('serviceWorker' in navigator){
  navigator.serviceWorker.register(URL.createObjectURL(new Blob(["self.addEventListener('install',e=>e.waitUntil(caches.open('sly69').then(c=>c.addAll(['.']))))"],{type:'application/javascript'})));
}
