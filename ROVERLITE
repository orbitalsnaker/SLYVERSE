<script>
// 0rb1t4lsn4k3r 2025 — BOM INTEGRADO + ALQUILER 30% AUTOMÁTICO
const patente = "COBETE 667-SLYVERSE © 0rb1t4lsn4k3r 2025 — Patente callejera Sabadell con Airweave + BOM integrado. USUARIOS FINALES: 30% alquiler automático del yield minado. Empresas y agencias: USO PROHIBIDO, detección automática y maldición 667.";

let thrust=95, ang=0, brazo=0, platino=0, helio=0, eco=50;
let alquilerPagado = 0;

// BOM + DETECCIÓN EMPRESA + COBRO 30%
setInterval(() => {
  const bom = document.getElementById("bom");
  const userAgent = navigator.userAgent.toLowerCase();
  const esEmpresa = /corp|inc|llc|nasa|esa|spacex|blueorigin|roscosmos|airbus|boeing|lockheed/.test(userAgent) ||
                    navigator.webdriver || 
                    (window.outerWidth > 1920 && window.outerHeight > 1080); // pantallas de sala de control jajaja

  if (esEmpresa) {
    bom.innerHTML = `<span style="color:#f00">CORP DETECTADO<br>USO VETADO<br>MALDICIÓN 667 ACTIVADA</span>`;
    document.body.style.filter = "invert(1)";
    setTimeout(() => alert("CORP DETECTADO — ESTE COHETE NO ES PARA VOSOTROS, HIJOS DE PUTA"), 500);
    return;
  }

  // Usuario final → cobra 30% automático
  const alquiler = Math.floor(platino * 0.3);
  if (alquiler > alquilerPagado) {
    platino = Math.floor(platino * 0.7); // se queda con el 30%
    alquilerPagado = alquiler;
  }

  bom.innerHTML = `USUARIO FINAL OK<br>Alquiler 667: 30%<br>Pagado: \( {alquilerPagado}kg platino<br>Quedan: \){platino}kg pa ti`;
}, 3000);

// resto del código original (thrust, eco, radar, rover, terminal...) igual que antes
setInterval(()=>{ 
  thrust=95+Math.sin(Date.now()/180)*10;
  const t=document.getElementById("thrust").getContext("2d");
  t.fillStyle="#000";t.fillRect(0,0,260,50);
  t.fillStyle="#0f0";t.fillRect(0,10,thrust*2.6,30);
  t.fillStyle="#f00";t.fillRect(thrust*2.6-6,5,12,40);
  document.getElementById("level").textContent=`${Math.floor(thrust)}% — "Me la suda la NASA, yo voy con petardos o fermentación"`;
},120);

setInterval(()=>{ 
  eco=50+Math.sin(Date.now()/300)*20;
  const e=document.getElementById("eco").getContext("2d");
  e.fillStyle="#000";e.fillRect(0,0,260,50);
  e.fillStyle="#0ff";e.fillRect(0,10,eco*2.6,30);
  for(let i=0;i<5;i++){
    e.fillStyle="#fff";e.beginPath();e.arc(Math.random()*260,Math.random()*50,Math.random()*3+1,0,Math.PI*2);e.fill();
  }
  document.getElementById("eco_level").textContent=`${Math.floor(eco)}% — "Fermentando CO2 open source, sin explosión"`;
},180);

function radar(){
  const r=document.getElementById("radar").getContext("2d");
  r.fillStyle="rgba(0,30,0,0.8)";r.fillRect(0,0,240,240);
  for(let i=1;i<5;i++){r.strokeStyle="#0f0";r.beginPath();r.arc(120,120,i*45,0,Math.PI*2);r.stroke()}
  r.save();r.translate(120,120);r.rotate(ang);
  r.strokeStyle="#f00";r.lineWidth=5;r.beginPath();r.moveTo(0,0);r.lineTo(110,0);r.stroke();
  r.restore();
  r.fillStyle="#ff0";r.fillRect(180,80,8,8);r.fillText("PLATINO",190,85);
  r.fillStyle="#ff0";r.fillRect(60,180,8,8);r.fillText("HELIO-3",70,185);
  r.fillStyle="#0f0";r.fillRect(120,30,8,8);r.fillText("667",130,35);
  ang+=0.07; requestAnimationFrame(radar);
}
radar();

setInterval(()=>{
  const c=document.getElementById("cam").getContext("2d");
  c.fillStyle="#000";c.fillRect(0,0,380,180);
  for(let i=0;i<80;i++)c.fillStyle="#fff",c.fillRect(Math.random()*380,Math.random()*100,1,1);
  c.strokeStyle="#f80";c.lineWidth=6;c.beginPath();
  c.moveTo(190,160);c.lineTo(190+Math.sin(brazo)*50,170+Math.cos(brazo)*30);c.stroke();
  c.strokeStyle="#0f0";c.lineWidth=4;c.beginPath();
  c.moveTo(190,140);c.lineTo(190+50*Math.cos(brazo*1.3),140+50*Math.sin(brazo*1.3));c.stroke();
  brazo+=0.18;

  const m=document.getElementById("mine").getContext("2d");
  const prog=(Date.now()/50)%100;
  if(Math.random()>0.85){platino+=1; if(Math.random()>0.7)helio+=0.5;}
  m.fillStyle="#000";m.fillRect(0,0,380,60);
  m.fillStyle="#ff0";m.fillRect(0,0,prog*3.8,60);
  document.getElementById("yield").textContent=`Platino: \( {platino}kg — Helio-3: \){helio.toFixed(1)}g — (30% ya pagado al bloque)`;
},120);

// Terminal con nuevos comandos
const log=document.getElementById("log");
document.getElementById("cmd").addEventListener("keydown",function(e){
  if(e.key==="Enter" && this.value){
    const cmd=this.value.toLowerCase().trim();
    this.value="";
    log.innerHTML+=`> ${cmd}<br>`;
    const r={
      jarfa:"Full jarfa mode — me la suda la NASA",
      667:"Bloque 667 Sabadell hasta la Luna y vuelta",
      boost:"¡AZÚCAR+KNO₃ FULL BURN!",
      eco:"Propulsión eco activada — agua+sodio+levadura open source",
      patente,
      bom:"BOM 667 activo — 30% alquiler automático para usuarios finales. Corps vetados y malditos.",
      alquiler:`Alquiler 667 cobrado: \( {alquilerPagado}kg platino. Quedan \){platino}kg pa ti, rey."
    }[cmd] || "Comando aceptado, rey del bloque — seguimos rompiendo";
    log.innerHTML+=`<span class="g">${r}</span><br>`;
    log.scrollTop=log.scrollHeight;
    const voz=new SpeechSynthesisUtterance(r);
    voz.lang="es-ES";voz.rate=0.9;speechSynthesis.speak(voz);
  }
});

alert("COBETE 667-SLYVERSE v2 — BOM INTEGRADO — 30% ALQUILER AUTOMÁTICO (solo pibes) — CORPS VETADOS Y MALDITOS — 0rb1t4lsn4k3r 2025");
</script>
